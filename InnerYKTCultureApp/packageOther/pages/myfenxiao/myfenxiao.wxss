/* packageOther/pages/myfenxiao/myfenxiao.wxss */
.swiper1 {
  width: 100%;
  height: 264rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.swiper1 .slide-image{
  width: 100%;
  height: 264rpx;
}
.banner{
  margin: 0 auto;
}

page{
  height:100%;
}
.tabbox {
  width: 33.3333%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  float: left;
  background: #fff;
}

.tab {
  display: inline-block;
  height: 52rpx;
  line-height: 52rpx;
  margin-top: 14rpx;
  box-sizing: border-box;
}

.tab:last-child {
  border-right: 0;
}
.topTabSwiper {
  /* position: fixed;
  left: 0;
  top: 0rpx; */
  width: 100%;
  z-index: 2;
  background: #eee;
  /* display: flex;
  justify-content: center;
  align-items: center; */
}

.topTabSwiper:after {
  content: "";
  clear: both;
  display: block;
}

.tabBorer {
  border-bottom: 2px solid #0066eb;
  color: #0066eb;
  box-sizing: border-box;
}

.swiper {
  width: 100%;
  /* position: fixed;
  left: 0;top: 80rpx; */
  padding-bottom: 20rpx;
  overflow: scroll;
}
swiper-item {
  overflow-y: scroll;
  position: relative;
  height: 100%;
}

.chose-txt{
  border-radius: 8px; 
  width: 100%;
  height: 200rpx; 
  padding: 20rpx 0;
  overflow: hidden;
  box-sizing: border-box;
  border-top: solid 2rpx #e5e5e5;
  clear: both;
}
.chose-txt:first-child{
  border-top: 0;
}
.cardimage{
  width: 160rpx;
  height: 160rpx;
  margin-left: 10rpx;
  border: solid 1rpx #e5e5e5;
  box-sizing: border-box;
  border-radius:10rpx; 
}
.card-nav1{
  width: 73%;
  height: 160rpx;
  margin-left: 12rpx;
  overflow: hidden;
}
.fx-brokerage{
  display: block;
  width: 100%;
  height: 38rpx;
  font-size: 28rpx;
  color: #ff1f1f;
  line-height: 38rpx;
}
.fx-brokerage text{
  font-size: 36rpx;
  font-family:'DIN';
}
.fx-price{
  display: block;
  width: 100%;
  height: 38rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 38rpx;
  margin-top: 6rpx;
}
.fx-price view{
  display: inline-block;
}
.fx-price view text{
  font-size: 36rpx;
  font-family:'DIN';
}
.fx-yuan{
  margin-right:20rpx;
  text-decoration:line-through;
  color: #999;
}
.fx-name{
  width: 100%;
  height: 72rpx;
  line-height: 36rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-top: 6rpx;
  word-break: break-all;
  font-size: 28rpx;
  color: #333;
}
.main{
  width: 94%;
  height:90%;position: relative;top:80rpx;

  overflow-y: scroll;
  left:3%;
  background: #fff;
  border-radius:10rpx; 
}
.FX-rank{
  width: 100%;
  height: 60rpx;
  background: #ececec;
  position: absolute;
  left: 0;top: 0;
}
.FX-rank .ranktype{
  display: inline-block;
  padding: 0 30rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.FX-rank .ranktype image{
  width: 28rpx;
  height: 24rpx;
  vertical-align: middle;
  margin-left: 12rpx;
  margin-top: -4rpx;
}
.searchbar{
  width:95%;height:70rpx;position:fixed;background: #f6f6f6; top:0;left:0;z-index: 99;
  padding: 0 2.5%;
  border-bottom:solid 2rpx #e5e5e5; 
}
.paixu{
  line-height: 60rpx;
  padding: 0 10rpx;
  margin-right: 20rpx;
  font-size: 28rpx;
  display: inline-block;
}
.searchbtn{
  width: 100%;
  height: 60rpx;
}
.searchbtn view.fleft{
  width: 68%;
  height: 60rpx;
  margin-top: 6rpx;
}
.searchbtn view.fleft text{
  
}
.searchbtn view.fleft image{
  width: 20rpx;
  height: 20rpx;
  margin-left: 8rpx;
  vertical-align: middle;
  margin-top: -4rpx;
}

.statistics{
  width: 690rpx;
  height: 222rpx;
  margin: 20rpx auto;
  position: relative;
}
.statistics image{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;top: 0;
  border-radius: 10rpx;
  z-index: 5;
}
.fx-addup,.fx-addup1{
  position: absolute;
  top: 60rpx;
  width: 100%;
  height: 162rpx;
  z-index: 10;
  text-align: center;
  padding: 0 2%;
  box-sizing:border-box;
  color: #fff;
}
.fx-addup{
  left: 0;
}
.fx-addup1{
  right: 0;
}
.fz-stats-name,.fz-withdraw{
  font-size: 28rpx;
  height: 30rpx;
  color: #d0e6d4;
}
.fz-stats-money{
  font-size: 28rpx;
  margin: 24rpx 0 20rpx 0;
  height: 30rpx;
  line-height: 30rpx;
}
.fz-stats-money text{
  font-size: 36rpx;
  margin-left: 8rpx;
  font-family: "DIN"
}
.fz-withdraw text{
  display: inline-block;
  border-bottom: solid 1rpx #d0e6d4;
}
.datalink{
  width:690rpx;
  margin: 30rpx auto;
}
.datalink view{
 width: 330rpx;
 height: 120rpx;
 line-height: 120rpx;
 background: #ececec;
 border-radius: 10rpx;
 color: #333;
 text-align: center;
}
.datalink view image{
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
  margin-top: -4rpx;
  margin-right:20rpx;
}
.fenxiaoimage{
  width: 100%;
}

.top-share-image{
  float: right;
  width: 30%;
  height: 60rpx;
  margin-top: 6rpx;
  overflow: hidden;
}
.shareimage{
  display: inline-block;
  float: right;
  width: 80rpx;
  height: 50rpx;
  /* margin:0!important;
  padding: 0!important; */
  margin-top: 4rpx!important;
  margin-left: 20rpx;
}
.sharebutton{
  display: inline-block;
  background: none;
  float: right;
  /* margin:0!important; */
  padding: 0!important;
  /* margin-right: 20rpx; */
}
.sharebutton image{
  display: inline-block;
  width: 80rpx;
  height: 50rpx;
}
.footertips{
  width: 100%;
  padding: 0 3%;
  box-sizing: border-box;
  position: fixed;
  bottom: 40rpx;
  left: 0;
  text-align: center;
}
.footertips text{
  display: inline-block;
  width: 100%;
  height: 40rpx;
  font-size: 28rpx;
  color: #999;
  text-align: left;
}
.footertips button{
  display: inline-block;
  padding: 0 60rpx;
  height: 60rpx;
  line-height: 56rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  border-radius: 30rpx;
  border: solid 2rpx #0066eb; 
  color: #0066eb;
  margin-top: 20rpx;
  background: none;
}
