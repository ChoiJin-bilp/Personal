/* pages/store/store.wxss */
page {
  background: #f6f6f6;
  font-family: DIN;
}

.shopbox {
  width: 100%;
  height: 100%;
  overflow-y: scroll;
  padding-bottom: 20rpx;
}

.merchant {
  width: 94%;
  height: 330rpx;
  border-radius: 16rpx;
  margin: 0 auto;
  background: #fff;
  margin-top: 20rpx;
  justify-content: space-between
}

.commodity {
  width: 94%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}

.commodity image {
  width: 206rpx;
  height: 206rpx;
  border-radius: 16rpx;
  overflow: hidden;
}

/* 瀑布流布局 */
.case-page {
  padding: 10rpx;
  display: flex;
  white-space: normal;
  justify-content: space-around;
}

.list-masonry {
  width: 48%;
}

.list-masonry:last-child {
  margin-bottom: 40rpx;
}

.store {
  width: 96%;
  padding: 0 2% 16rpx 2%;
  height: auto;
  margin-top: 20rpx;
  border-radius: 4px;
  background: #fff;
}

.store image.shopimg {
  width: 100%;
}

