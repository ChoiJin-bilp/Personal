  background-color: #fff;
}

.search input {
  width: 84%;
  padding: 0 2%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #333;
  position: absolute;
  left: 0;
  top: 0;
}

.search image {
  width: 40rpx;
  height: 40rpx;
  padding: 12rpx 20rpx;
}

.searchbtn {
  width: 100%;
  height: 60rpx;
}

.searchbtn view.fleft {
  width: 58%;
  height: 60rpx;
  margin-top: 6rpx;
}

.searchbtn view.fleft image {
  width: 20rpx;
  height: 20rpx;
  margin-left: 8rpx;
  vertical-align: middle;
  margin-top: -4rpx;
}

.paixu {
  line-height: 60rpx;
  padding: 0 10rpx;
  margin-right: 20rpx;
  font-size: 32rpx;
  display: inline-block;
  font-weight: bold;
}

.searchbtn image.fright {
  width: 40rpx;
  height: 30rpx;
  padding: 18rpx;
}


/* 黄夏迪部分样式 */

.wx-popup {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.281);
  z-index: 999;
}

.popup-container {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 80%;
  max-width: 700rpx;
  border: 2rpx solid #ccc;
  border-radius: 10rpx;
  box-sizing: bordre-box;
  transform: translate(-50%, -50%);
  overflow: hidden;
  background: #fff;
}

.wx-popup-text {
  /* font-weight: bold; */
  height: 40rpx;
  line-height: 40rpx;
  padding: 20rpx 0;
  color: #999;
  font-size: 30rpx;
  margin: 0 auto;
  width: 94%;
  clear: both;
}

.wx-popup-btntype0 {
  width: 100%;
}

.wx-popup-btntype2 text {
  float: left;
  margin: 10rpx 5%;
  width: 40%;
  height: 50rpx;
  border: 2rpx solid #666;
  border-radius: 10rpx;

}

.wx-popup-btntype {}

.wx-popup-btntype text {
  display: inline-block;
  padding: 0 40rpx;
  height: 60rpx;
  line-height: 56rpx;
  border: 2rpx solid #e5e5e5;
  box-sizing: border-box;
  border-radius: 30rpx;
  font-size: 32rpx;
  margin-left: 3%;
}

.wx-popup-input {}

.wx-popup-input input {
  border: 2rpx solid #e5e5e5;
  width: 30%;
  display: inline-block;
  border-radius: 30rpx;
  height: 60rpx;
  line-height: 56rpx;
  margin-left: 3%;
  padding: 0 20rpx;
  font-size: 32rpx;
}

.wx-popup-input text {
  display: inline-block;
  width: 30rpx;
  height: 2rpx;
  background: #999;
  margin: -28rpx 0 28rpx 3%;
}

.wx-popup-btn {
  display: flex;
  justify-content: space-around;
  margin-top: 60rpx;
  width: 100%;
  height: 120rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}

.wx-popup-btn text {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 40rpx;
  height: 60rpx;
  margin-top: 30rpx;
  line-height: 60rpx;
  border: 2rpx solid #0066eb;
  border-radius: 30rpx;
  color: #0066eb;
}

