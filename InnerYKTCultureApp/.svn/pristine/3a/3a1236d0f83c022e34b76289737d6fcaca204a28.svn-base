/* packageSMall/pages/myservice/myservice.wxss */
page {
  height: 100%;
  background: #f6f6f6;
}
.width94 {
  width: 96%;
  /* background: #fff; */
  margin: 20rpx auto;
  border-radius: 16rpx;
  overflow: hidden;
  line-height: 72rpx;
  font-size: 28rpx;
  color: #666;
}
.cart-box {
  padding: 10rpx 0;
  overflow: hidden;
  margin-top: 10px;
  border-radius: 16rpx;
  background: #fff;
}
.cart-box:first-child {
  margin-top: 0px;
}

.cart-box2 {
  padding-top: 50rpx;
}

.cart-list-one {
  /* position: relative; */
  padding: 8rpx 0;
  height: 270rpx;
  width: 100%;
  overflow: hidden;
  background: #fff;
}

.astoredetails {
  width: 94%;
  margin: 12rpx auto;
  height: 164rpx;

}

.cart-thumb-one {
  float: left;
  width: 164rpx;
  height: 164rpx;
  border: solid 2rpx #f8f8f8;
  box-sizing: border-box;
  border-radius: 12rpx;
}

.cart-infor-one {
  height: 164rpx;
  width: 480rpx;
  float: left;
  margin-left: 20rpx;
}

.cart-moeny-one {
  width: 100%;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 30rpx;
  color: #333;
}

.cart-pro-price-one {
  display: inline-block;
  color: #ff1f1f;
  font-size: 24rpx;
}

.cart-count-box-one {
  float: right;
  width: 220rpx;
  height: 46rpx;
}

.cart-list .cart-thumb {
  position: absolute;
  top: 10rpx;
  left: 20rpx;
  width: 164rpx;
  height: 164rpx;
  border: solid 2rpx #f8f8f8;
  box-sizing: border-box;
  border-radius: 12rpx;
}
.storenumber {
  width: 96%;
  padding: 0 2%;
  margin: 0 auto;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: #999;
  background: #fff;
  padding-top: 20rpx;
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
}
.cart-list .cart-pro-name {
  display: inline-block;
  width: 100%;
  height: 36rpx;
  line-height: 36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
  font-weight: bold;
  color: #000;
}
.cart-norms{
  width: 100%;
  height: 36rpx;
  line-height: 36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
  color: #333;
}

.cart-moeny {
  width: 100%;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 30rpx;
  color: #333;
}

.cart-list .cart-pro-price {
  display: inline-block;
  color: #ff1f1f;
  font-size: 24rpx;
}

.cart-list .cart-pro-price text {
  font-size: 32rpx;
  font-weight: bold;
}

.cart-list .cart-count-box {
  float: right;
  width: 220rpx;
  height: 46rpx;
}

.cart-list .cart-count-box text {
  display: inline-block;
  vertical-align: middle;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
}
.orderdetails {
  width: 96%;
  padding: 0 2%;
  height: 72rpx;
  clear: both;
  margin: 0 auto;
  background: #fff;
  border-top: 1rpx solid #e5e5e5;
}
.orderdetails:last-child{
  border: none;
}
.orderdetails text {
  float: left;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 28rpx;
  color: #666;
}
.orderdetails .total{
  height: 72rpx;
  line-height: 72rpx;
  font-family: 'DIN';
  font-size: 34rpx;
  color: #ff1f1f;
}
.orderdetails .total text{
  font-size: 28rpx;
  color: #000;
  margin-right: 4rpx;
}
.orderdetails  view.fright{
  color: #ff3434;
}
.ashopname {
  padding: 8rpx 3% 10px 3%;
}

.reason{
  width: 96%;
  padding: 0 2% 20rpx 2%; 
}
.reason .section{
  width: 100%;
  clear: both;
  padding: 10rpx 0;
}
.section__title{
  float: left;
  width: 22%;
  height: 72rpx;
  line-height: 72rpx;
}
.picker{
  float: left;
  width: 76%;
  height: 72rpx;
  line-height: 72rpx;
  border: solid 2rpx #e5e5e5;
  box-sizing: border-box;
  border-radius:4px; 
  padding: 0 1%;
  position: relative;
}
.section textarea{
  float: left;
  width: 76%;
  height: 180rpx;
  border: solid 2rpx #e5e5e5;
  box-sizing: border-box;
  border-radius: 4px;
  margin: 10rpx auto;
  margin-bottom: 0;
  line-height: 40rpx;
  font-size: 28rpx;
  padding: 0 1%;
}
.text-box{
  display: inline-block;
  width: 76%;
  height: 180rpx;
  border: solid 2rpx #e5e5e5;
  box-sizing: border-box;
  border-radius: 4px;
  margin: 10rpx auto;
  margin-bottom: 0;
  line-height: 40rpx;
  overflow: hidden;
  overflow-y: scroll;
  word-wrap:break-word;
  text-align: justify;
  font-size: 28rpx;
  padding: 0 1%;
}
.addimg1{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  left: 50%;top: 50%;
  margin: -30rpx -30rpx;
}
.otherimg{
  display: flex;
 flex-wrap:wrap;
 justify-content:flex-start;
}
.firstimg{
  width: 222rpx;
  height: 98rpx;
  border-radius: 16rpx;
  background: #e9e9e9;
  position: relative;
  float: left;
  margin-top:20rpx;
  margin-right:12rpx; 
}
.scrollimg{
  width: 100%;
  height: 98rpx;
  border-radius: 16rpx;
}
.submit{ 
  width: 100%;
  height: 72rpx;
  padding: 40px 0 20px;
  text-align: center;
}
.submit button{
  display: inline-block;
  padding: 0 30px;
  height: 72rpx;
  line-height: 68rpx;
  font-size: 30rpx;
  color: #0066eb;
  border: solid 2rpx #0066eb;
  border-radius: 36rpx;
}
.order-btn {
  float: right;
  height: 72rpx;
}
.order-btn button {
  display: inline-block;
  background: #fff;
  height: 52rpx;
  line-height: 48rpx;
  margin-top: 10rpx;
  border: solid 2rpx #136fe6;
  box-sizing: border-box;
  font-size: 28rpx;
  padding: 0 26rpx;
  border-radius: 26rpx;
  color: #136fe6;
  margin-left: 20rpx;
}
.order-btn button:first-child {
  margin-left: 0;
}