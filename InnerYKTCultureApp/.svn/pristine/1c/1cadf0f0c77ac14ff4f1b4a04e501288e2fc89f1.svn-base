/* packageVP/pages/CYmapdepot/CYmapdepot.wxss */
.viewforTop{
  position: absolute;
  top: 0;
  width: 96%;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
}
.viewforTop2{
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  overflow-x: auto;
}
.viewfor{
  white-space: nowrap;
  background: #e5e5e5;
border-radius: 4rpx;
font-size: 28rpx;
color: #666666;
line-height: 36rpx;
padding: 6rpx 20rpx;
margin-left: 20rpx;
margin-top: 10rpx;
}
.viewforhu{
  background: #018766;
color: #fff;
}
.pltur{
  padding: 0rpx 20rpx;
  box-sizing: border-box;
  margin-top:50rpx;
}
.selectCup {
  width: 100%;
  display: flex;
  flex-wrap:  wrap;
}
.Cup-list2{
  width: 100%;
  display: flex;
  height: auto;
  flex-wrap: wrap;
  overflow-x: auto;
}
.Cup-z2{
  float: left;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-left: 8rpx;
  margin-right: 8rpx;
  margin-top: 20rpx;
  padding: 10rpx 10rpx;
  width: 140rpx;
}
.Cup-z2Succ{
  background: #edffff;
  border: 2rpx solid #018766;
  border-radius: 12rpx;
}
.Cup-z2::after{
  clear: both;
}
.Cup-z2 image{
  margin-top: 10rpx;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
}
.numbop{
  display: flex;
  font-size: 24rpx;
font-family: DIN, DIN-Regular;
color: #666666;
align-items: center;
margin-top:20rpx;
}
.numbop image{
  width: 20rpx;
  height: 20rpx;
  margin-top:0rpx;
  border-radius: 0%;
  margin-right:10rpx;
}
.Cup-text2{
  font-size: 22rpx;
color: #333333;
line-height: 36rpx;
text-align: center;
}
.Cup-text2input{
  font-size: 22rpx;
  color: #333333;
  line-height: 36rpx;
  text-align: center;
  border: #ccc solid 2rpx;
}
.lfrpimg{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.lfrpimg image{
  width: 30rpx;
  height: 30rpx;
  border-radius: 0;
}
.plfyurt{
  border-radius: 50%;
  background: #cccccc;
}
.viewfor image{
  width: 30rpx;
height: 27rpx;
}