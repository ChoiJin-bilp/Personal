/* pages/perfect/perfect.wxss */
page{
  padding-bottom:40rpx; 
}
.show{
  display: block;
}
.hide{
  display: none;
}
.xiala{
  width: 28rpx;
  height: 16rpx;
  position: absolute;
  right: 10rpx;
  top: 30rpx;
}
.perfect-list{
  width: 94%;
  margin: 0 auto;
  clear: both;
  line-height: 60rpx;
  padding: 10rpx 0;
}
.perfect-list text{
  width: 20%;
  margin-right: 3%;
  font-size: 28rpx;
  color: #666;
  font-weight:500;
  height: 60rpx;
  text-align: justify;
}
.perfect-list text::after{
  width: 100%;
  display: inline-block;
  content: '';
}
.perfect-list input{
  width: 77%;
  color: #333;
  height: 60rpx;
  font-size: 28rpx;
  border-bottom: solid 2rpx #f5f5f5;
}
.introduction{
  width:96%;
  color: #333;
  line-height: 40rpx;
  min-height: 60rpx;
  padding: 10rpx 2%;
  font-size: 28rpx;
  border: solid 2rpx #e5e5e5;
  border-radius: 16rpx;
  overflow: hidden;
} 
.save{
  position: fixed;
  width: 100%;
  height: 100rpx;
  left: 0;
  bottom: 0;
  background: #fff;
  text-align: center;
}
.btn-box{
  width: 100%;
  text-align: center;
}
.btn-box button.btn-foot{
  display: inline-block;
  border: solid 2rpx #0066e9;
  box-sizing: border-box;
  padding:0 60rpx; 
  margin-left: 8%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #0066e9;
  margin-top: 20rpx;
  border-radius:80rpx;
  font-size: 28rpx; 
}
.btn-box button.btn-foot:first-child{
  margin-left: 0;
}
.btn-enable{
  color: #0066eb;
  border: solid 2rpx #0066eb;
}
.btn-disable{
  color: #999;
  border: solid 2rpx #999;
}
.bgwhite{
  background: #fff;
  color: #666;
  border: solid 2rpx #999;
  box-sizing: border-box;
}
.bgblue{
  background: #0066eb;
  color: #fff;
  border: solid 2rpx #0066eb;
  box-sizing: border-box;
}


.required{
  display: block;
  width: 40rpx;
  height: 40rpx;
  margin-left: 0rpx;
  float: left;
  color: red!important;
}
.title-name{
width: 96%;
height: 80rpx;
line-height: 80rpx;
padding: 0 2%;
font-size: 30rpx;font-weight: bold; 
border-bottom: solid 2rpx #f5f5f5;
}

.up-image{
  float: left;
  width: 160rpx;
  height: 120rpx;
  border: solid 2rpx #f5f5f5;
  border-radius: 8rpx;
  margin-top: 10rpx;
}
/* .up-image image{
  width: 100%;
  height: 100%;
} */

.row{
    flex: 1;
    display: flex;
    width: 76%;
    height:100%;
    margin: 0 5%;
    overflow: hidden;  
}
.row .text{
    flex: 1;
    width: 96%;
    height: 240rpx;
    font-size: 28rpx;
    line-height: 22px;
    color: #333;
    border: solid 2rpx #f5f5f5;
    border-radius: 16rpx;
    padding: 12rpx 2%;
}
/*checkbox 选项框大小  */
.checkbox-box{
  width: 92%;
  padding:0 4%; 
  font-size: 28rpx;
  line-height: 40rpx;
  color: #666;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
}
checkbox .wx-checkbox-input {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.add-image{
  margin-top: 20rpx;
}
.image-box{
  float: left;
  width: 160rpx;
  height: 120rpx;
  border-radius: 8rpx;
  position: relative;
}
/* .image-box-company{
  float: left;
  width: 70%;
  background: red;
} */
.photo,.add-image{
  float: left;
  display: block;
  position: absolute;
  width: 160rpx;
  height: 120rpx;
  left: 0;
  top: -20rpx;
  border-radius: 8rpx;
  overflow: hidden;
}
.del-img{
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  right: -18rpx;
  top: -18rpx;
}
.btn-foot1{
  height: 60rpx;
  line-height: 56rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  border-radius: 60rpx;
  background: #fff;
  text-align: center;
  width: 28%;
  margin-left:0 auto;
}

.image-box-comp{
  display: block;
  float: left;
  width: 200rpx;
  height: 140rpx;
  border-radius: 8rpx;
  position: relative;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
}
.add-image-comp{
  float: left;
  width: 160rpx;
  height: 120rpx;
  margin-top: 20rpx;
}
.photo{
  position: absolute;
  width: 200rpx;
  height: 140rpx;
  left: 0;
  top: 0;
  /* border: solid 2rpx #f5f5f5; */
  border-radius: 8rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.del-img{
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  right: -18rpx;
  top: -18rpx;
}
.other-input-image{
  clear: both;
  width: 100%;
  margin: 0 auto;
}
.comp-image{
  width: 100%;
}
.default{
  display: inline-block;
  padding: 0 40rpx;
  background: #fff;
  height: 60rpx; 
  line-height: 60rpx;
  font-size: 28rpx;
  border-radius: 60rpx;
  color: #0066eb;
  border: #0066eb solid 2rpx;
}

.foot-company{
  width:92%;
  padding: 8rpx 4%;
  height:100rpx;
  margin: 0 auto; 
  margin-bottom: 20rpx;
  border-bottom: solid 2rpx #f5f5f5;
}
.use-data{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 0.7828rpx;
  color: #333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.use-data text:first-child{
  margin-right: 40rpx;
}
.use-company{
  height: 46rpx;
  line-height: 46rpx;
  font-size: 0.7828rpx;
  color: #333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.section{
  float: right;
  width: 77%;
  height: 60rpx;
  color: #333;
  font-size: 28rpx;
  border-bottom: solid 2rpx #f5f5f5;
}

.nocompany{
  width: 100%;
  height: 100%;
  background: red;
}
.nocompany view{
  width: 100%;
  height: 60rpx;
  position: absolute;
  left: 0;
  top: 30%;
  text-align: center;
  font-size: 32rpx;
  color: #999;
  line-height: 60rpx; 
}
.nocompany button{
  width: 30%;
  height:72rpx;
  position: absolute;
  left: 35%;
  top: 38%;
  text-align: center;
  font-size: 32rpx;
  line-height: 68rpx;
  border: solid 2rpx #0066eb;
  color: #0066eb;
  background: white;
  border-radius: 36rpx;
}
/* 授权 */
.authorization{
  width:80%;
  position: absolute;
  background: #fff;
  top:50%;left:50%;
  padding: 30rpx 3% 50rpx 3%;
  transform: translate(-50%,-50%);
  z-index: 10060;
  border-radius: 8px;
}
.title-top{
  width: 94%;
  font-size: 128rpx;
  font-weight: 500;
  color: #333;
  margin: 0 auto;
  margin-bottom: 40rpx;
}
.poptextnav{
  width: 94%;
  line-height: 52rpx;
  font-size: 28rpx;
  margin: 0 auto;
  color: #666;
}
.iknow{
  display: inline-block;
  height: 64rpx;
  padding: 0 60rpx;
  margin-top: 40rpx;
  line-height: 60rpx;
  font-size: 0.8528rpx;
  border-radius: 32rpx;
  background: #fff;
  border: solid 2rpx #0066eb;
  color: #0066eb;
  margin-left: 20%;
}
.iknow:first-child{
   margin-left: 0;
}
.pop-foot{
  height: 100rpx;
}
.pop-foot text{
  display: block;
  width: 35%;
  height:60rpx;
  line-height: 56rpx;
  margin: 20rpx 7.5%;
  text-align: center;
  border-radius: 60rpx;
  float: left;
}