/* packageYK/pages/statShare/statShare.wxss */
page {
  width: 100%;
  height: 100%;
}
.fcton{
  padding: 20rpx;
}
.fcton-t{
  display: flex;
  justify-content: space-between;
}
.fcton-t input{
  font-size: 32rpx;
  border-radius: 8rpx;
  height: 60rpx;
  border:#000 solid 2rpx;
  position: relative;
}
.fcton-t-o{
  position: relative;
  box-sizing: border-box;
  width: 44%;
}
.fcton-t-o input{
  padding-left: 20rpx;
  box-sizing: border-box;
}
.xialaw{
  z-index: 20;
  position: absolute;
  background: #fff;
  top: 60rpx;
  right:0rpx;
  width: 88%;
  height: 270rpx;
  border: #000 2rpx solid;
  transition: height 1s;
  padding: 0 10rpx;
  overflow-y: auto;
}
.fcton-t-t{
  box-sizing: border-box;
  width: 60%;
  height: 80rpx;
  
  position: relative;
}
.fcton-t-t input{
  padding-left: 80rpx;
  padding-right: 60rpx; 
  box-sizing: border-box;
}
.fcton-t-t1{
  box-sizing: border-box;
  width: 100%;
  height: 80rpx;
  
  position: relative;
}
.fcton-t-t1 input{
  padding-left: 8%;
  box-sizing: border-box;
}
.fcton-body{
  font-size: 34rpx;
  display: flex;
  margin-top: 20rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  width: 100%;
  height: 260rpx;
  position: relative;
}
.fcton-body-dw{
  color: #fff;
  font-size: 34rpx;
  display: flex;
  border-radius: 10rpx;
  box-sizing: border-box;
  width: 100%;
  z-index: 3;
  position: absolute;
}
.fcton-body-f{
  padding-top: 10rpx;
  box-sizing: border-box;
  width: 50%;
  height: 100%;
}
.fcton-body-f view{
  text-align: center;
  margin:  0 auto;
  margin-top: 30rpx;
  width: 80%;
}
.gimage{
  z-index: 1;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.padd{
  padding-right:6rpx; 
  white-space:nowrap;
}
.fcton-title{
  display: flex;
  justify-content: space-around;
  line-height: 80rpx;
  font-size: 28rpx;
  align-items: center;
  color: #000;
  border-bottom: solid #ccc  2rpx;
}
.fcton-title view{
  width: 25%;
  text-align: center;
  overflow:hidden;
  white-space:nowrap;
}
.zhuandong{
  transform:rotate(180deg);
}
.chaochu{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.cent{
  text-align: center;
}
.xiala{
  width: 100%;
}
.xiala view{
  width: 100%;
  position: absolute;
  height: 1000rpx;
  border-bottom: solid #ccc  2rpx;
  bottom: 0;
  left: 0;
}
.font{
  font-size: 32rpx;
}
.font-o{
  font-size: 36rpx;
  font-weight: bold;
}
image{
  margin: 0;
  padding: 0;
}
.search{
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  height: 40rpx;
  width: 40rpx;
  z-index: 40;
}
.searche{
  position: absolute;
  right: 13rpx;
  top: 24rpx;
  height: 20rpx;
  width: 30rpx;
  transition: transform 0.5s;
  z-index: 40;
}
.xialaw view{
  text-align: center;
  line-height: 60rpx;
  border-bottom: #ccc solid 2rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  box-sizing: border-box;
  overflow: hidden;white-space: nowrap;text-overflow: ellipsis;
}
.xialaw2 view{
  text-align: center;
  line-height: 60rpx;
  border-bottom: #ccc solid 2rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  box-sizing: border-box;
  overflow: hidden;white-space: nowrap;text-overflow: ellipsis;
}
.xialaw2{
  z-index: 20;
  position: absolute;
  background: #fff;
  top: 60rpx;
  right:-12rpx;
  width: 100%;
  height: 270rpx;
  border: #000 2rpx solid;
  transition: height 1s;
  padding: 0 10rpx;
  overflow-y: auto;
}