/* pages/shopdetail/shopdetail.wxss */
page{
  background: #f6f6f6;
  height:100%;
}
.yesshop{
   width: 92%;
   margin: 0 auto;
   padding: 20rpx 4%;
 }
 /* 轮播 */
.swiper {
  width:100%;
  height: 264rpx;
  margin: 20rpx auto;
  border-radius: 16rpx;
  overflow: hidden;
}
.slide-image{
  width: 100%;
  height:100%;
 }

 .store-name{
   width: 100%;
   height: 60rpx;
   padding: 20rpx 0;
   background: #b9e1ff;
   border-radius: 16rpx;
   text-align: center;
 }
.store-name image{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  border-radius:50%;
  overflow: hidden; 
  margin-right: 20rpx;
  background: white;
  margin-top: -6rpx;
}
.store-name text{
  display: inline-block;
  line-height: 60rpx;
  margin-right: 20rpx;
  font-size: 30rpx;
}
.store-name view{
  display: inline-block;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 10rpx;
  border: 2rpx solid #ff008e;
  padding: 0 12rpx;
  color: #ff008e;
  border-radius: 8rpx;
  font-size: 28rpx;
}
/*查询条件部分*/
.searchbar{
  width:100%;
  height:76rpx;
}
.search{
  width: 80%;
  height: 76rpx;
  border-radius: 12rpx;
  border: solid 2rpx #e5e5e5;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
}
.search input{
  width: 82%;
  padding: 0 2%;
  height: 76rpx;
  line-height: 72rpx;
  font-size: 28rpx;
  color: #333;
  position: absolute;
  left:0;
  top: 0;
}
.search image{
  width: 40rpx;
  height: 40rpx;
  padding: 18rpx;
}
.screen{
  float: right;
  width: 19%;
  height: 76rpx;
  line-height: 76rpx;
  text-align: right;
}
.screen image{
  width: 26rpx;
  height: 16rpx;
  vertical-align: middle;
  margin-left: 8rpx;
}
.cart-box{
    width:100%;
}
.cart-list{
    position: relative;
    width: 100%;
    padding: 20rpx 0;
    height: 200rpx;
    background: white;
    margin-top: 20rpx;
    border-radius: 16rpx;
    border-bottom-right-radius: 16rpx;
    overflow: hidden;
}
.cart-list .cart-thumb{
    position: absolute;
    top:20rpx;
    left: 20rpx;
    width: 164rpx;
    height: 164rpx;
    border: solid 2rpx #f8f8f8;
    box-sizing: border-box;
    border-radius: 12rpx;
}
.Goodstate{
  position: absolute;
  bottom: 20rpx;
  left: 20rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: #666;
}
.cart-infor{
  width:470rpx;
  height: 164rpx;
  position: absolute;
  left: 204rpx;
  top: 20rpx;
}
.cart-list .cart-pro-name{
  display: inline-block;
  width: 100%;
  height: 72rpx;
  line-height: 36rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
  font-weight: bold;
  color: #000;
}
.cart-norms{
  width: 100%;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 30rpx;
  color: #333;
}
.cart-moeny{
  width: 100%;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 30rpx;
  color: #333;
}
.cart-list .cart-pro-price{
    display: inline-block;
    color: #ff1f1f;
    font-size: 24rpx;
}
.cart-list .cart-pro-price text{
    font-size: 32rpx;
    font-weight: bold;
}
.store {
  
  
  background:white;
  margin: 20rpx 0 0 4%;
  border-radius: 16rpx;
  text-align: center;
  /* position: relative;
  overflow: hidden; */
}
.store{
  width: 44%;
  height: 500rpx;
  float: left;
  margin: 20rpx 0 0 4%;
  border-radius: 16rpx;
  text-align: center;
  background: #fff;
}
.store image.shopimg {
  width: 100%;
  height: 336rpx;
}

.shopping {
  width: 100%;
  height: 160rpx;
}
.shopname {
  width: 94%;
  padding: 0 3%;
  height: 72rpx;
  line-height: 36rpx;
  text-align: left;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
  font-weight: bold;
  color: #000;
}

.shoppay {
  width: 94%;
  padding: 0 3%;
  height: 36rpx;
  line-height: 36rpx;
  text-align: left;
}
.sotrename{
  width: 94%;
  padding: 0 3%;
  height: 44rpx;
  line-height: 44rpx;
  text-align: left;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size: 26rpx;
  color: #999;
  margin-top: 4rpx;
}
.sotrename text{
  border: 2rpx solid #ff008e;
  padding: 0 12rpx;
  height: 40rpx;
  line-height: 38rpx;
  color: #ff008e;
  margin-right:6rpx;
  border-radius: 8rpx;
}
.selling, .original {
  display: inline-block;
}

.selling {
  font-size: 26rpx;
  color: #ff1f1f;
}

.selling text {
  font-size: 32rpx;
  font-weight: bold;
  padding: 0 0 0 4rpx;
}

.original {
  font-size: 26rpx;
  color: #666;
  margin-left: 20rpx;
  text-decoration: line-through;
}
/* 黄夏迪部分样式 */

.wx-popup {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.281);
  z-index: 999;
}

.popup-container {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 80%;
  max-width: 700rpx;
  border: 2rpx solid #ccc;
  border-radius: 10rpx;
  box-sizing: bordre-box;
  transform: translate(-50%, -50%);
  overflow: hidden;
  background: #fff;
}
.wx-popup-text{
  margin-top: 20rpx;
  margin-left: 10rpx;
  margin-bottom: 10rpx;
  font-weight: bold;
}
.wx-popup-btntype0 {
  width:100%;
  height:30%;
  overflow-y:scroll;
  margin-bottom: 20rpx;
}

.wx-popup-btntype2 text {
  float: left;
  margin: 10rpx 5%;
  width: 40%;
  height: 50rpx;
  border: 2rpx solid #666;
  border-radius: 10rpx;
}
.wx-popup-btntype {
  display: flex;
  justify-content: space-around;
  margin-bottom: 20rpx;
}

.wx-popup-btntype text {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30%;
  height: 50rpx;
  border: 2rpx solid #666;
  border-radius: 10rpx;
  font-size: 32rpx;
}

.wx-popup-input {
  display: flex;
  justify-content: space-around;
  margin-bottom: 20rpx;
}

.wx-popup-input input {
  border: 2rpx solid #999898;
  width: 40%;
  border-radius: 10rpx;
  height: 50rpx;
  margin-left: 10rpx;
  padding-left: 10rpx;
  font-size: 32rpx;
}

.wx-popup-btn {
  display: flex;
  justify-content: space-around;
  margin-top: 80rpx;
  margin-bottom: 20rpx;
}

.wx-popup-btn text {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30%;
  height: 60rpx;
  line-height: 60rpx;
  border: 2rpx solid #999898;
  border-radius: 88rpx;
}
/* 弹窗 */
.userule{
  position: fixed;
  left: 0;top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
}
.bgopcity{
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.4;
  position: absolute;
  left: 0; top: 0;
  z-index: 1050;
}
.recordbox{
  width:94%;
  height:65%;
  position: relative;
  top:50%;left:50%;
  padding: 10rpx 0;
  transform: translate(-50%,-50%);
  z-index: 9999;
  overflow-y:scroll;
  overflow: hidden;
}
.record{
  width:100%;
  height:93%;
  position: absolute;
  top:0;left:0;
  background-color: #fff;
  border-radius: 8px;
  overflow-y:scroll;
}
.tips-name{
  height: 80rpx;
  line-height: 80rpx;
  font-weight: bold;
  font-size: 0.9rem;
}
.chose-txt{
  display: inline-block;
  border-radius: 12rpx; 
  height: 64rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  margin-top: 20rpx;
  padding: 0 16rpx;
  margin-left: 12rpx;
  box-sizing: border-box;
}
.chose-txt1{
  display: inline-block;
  border-radius: 12rpx; 
  height: 64rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  margin-top: 20rpx;
  padding: 0 16rpx;
  margin-left: 12rpx;
  box-sizing: border-box;
  font-weight: bolder;
  color: #136fe6;
}
.tips{
  width: 94%;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #333;
  margin: 0 auto;
  background: #fff;
  border-radius: 16rpx;
}
.tips text{
  display: inline-block;
  width: 94%;
  line-height: 40rpx;
  margin: 0 auto;
  font-size: 28rpx;
  color: #666;
  padding: 20rpx;
  text-align: justify;
}