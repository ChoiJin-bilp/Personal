.comp-name{
  height: 100rpx;
  line-height: 60rpx;
  font-size: 40rpx;
  font-weight: bold;
  width: 100%;
  padding: 20rpx 3%;
  box-sizing: border-box;
}
.comp-name view{
  display: inline-block;
  width: 100rpx;
  height: 60rpx;
}
.comp-name text{
  height: 60rpx;
  display: inline-block;
}
.comp-name image{
  width: 100%;
  height: 60rpx;
  vertical-align: top;
  margin-right: 12rpx;
}
/* 轮播 */
.swiper {
  width: 100%;
  height: 360rpx;
  /* border-radius: 20rpx; */
  overflow: hidden;
}
.banner{
  height: 360rpx;
}
.slide-image{
  width: 100%;
  height: 360rpx;
}
.banner{
  margin: 0 auto;
}
/* 轮播 */
.swiper1 {
  width: 100%;
  height:360rpx;
  /* border-radius: 20rpx; */
  overflow: hidden;
}
.slide-image1{
  width: 100%;
  height: 360rpx;

}
.banner1{
  margin: 20rpx auto;
}

.Information{
  width: 94%;
  margin: 0 auto;
  clear: both;
  border-bottom: solid 2rpx #e5e5e5;
  padding: 20rpx 0;
  font-size: 30rpx;
}
.Information view.view1{ 
  color: #000;
  font-size: 32rpx;
  /* font-weight: bold; */
}
.Information view.view1 text{
  color: #333;
  font-size: 32rpx;
  font-weight: normal;
}
.Information view.view1 image{
  width: 36rpx;
  height: 34rpx;
  vertical-align: middle;
  float: right;
  padding: 8rpx 0;
}
/* 切换信息 */
.footmenu{
  width: 100%;
  height: 80rpx;
  padding: 10rpx 3%;
  box-sizing: border-box;
  border-bottom: solid 2rpx #e5e5e5;
}
.box_list{
  display: inline-block;
  float: left;
  height: 60rpx;
  padding-right: 40rpx;
  padding-left: 40rpx;
  border-left: solid 2rpx #e6e6e6;
}
.box_list:first-child{
  padding-left: 0;
  border-left: none;
}
.box-image{
  height: 48rpx;
  width: 48rpx;
  padding-top: 6rpx;
  vertical-align: top;
  margin-right: 10rpx;
}
.box-text{
  display: inline-block;
  font-size: 32rpx;
  color: #666;
  height: 60rpx;
  line-height: 60rpx;
  font-weight:bold;
}
.tabBorer text{
  font-size: 34rpx;
  color: #0066eb;
}
.active{
  color: #666;
  font-weight: bold;
}
/* 切换页面 */
.changeview{
  width: 94%;
  
  padding: 10rpx 0;
  margin: 0 auto;
  overflow: hidden;
  overflow-y: scroll;
}
.changeview text.hotelintroduce{
  display: inline-block;
  width: 100%;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #666;
  text-align:justify; 
}
.changeview image.hotelintroduce{
  display: inline-block;
  width: 100%;
  margin-top: 20rpx;
}
/* 日历 */
.selection{
  clear: both;
  margin: 0 auto;
  height: 80rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
}

.selection .fleft{
  width: 43%;
  height: 60rpx;
  line-height: 60rpx;
  color: #666;
  font-size: 30rpx;
}
.selection .fleft .viewname{
  float: left;
  width: 24%;
  height: 60rpx;
  line-height: 60rpx;
  color: #0066eb;
}
.selection .fleft .viewdate{
  float: right;
  width: 74%;
  margin-right: 2%;
  color: #000;
  font-size: 34rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: solid 1rpx #e5e5e5;
}

.selection .fright{
  width: 14%;
  height: 60rpx;
  line-height: 60rpx;
  color: #666;
  font-size: 28rpx;
  text-align: right;
}
.selection .fright text{
  padding: 0 4rpx;
  color: #0066eb;
  font-size: 32rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 10rpx;
  border-bottom: solid 1rpx #e5e5e5;
}
/* 酒店列表 */
.showhotle{
  width: 100%;
  height: 510rpx;
  background: #fff;
  /* border-radius: 10px; */
  overflow: hidden;
  margin-top: 20rpx;
}
.showhotle .hotelimage{
  width: 100%;
  height: 280rpx;
}
.navview{
  width: 100%;
  padding: 0 2%;
  box-sizing: border-box;
  height:54rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.navview .hotelprice{
  display: inline-block;
  height:54rpx;
  line-height: 54rpx;
  margin-right: 20rpx;
}
.navview view.hotelprice{
  font-size: 32rpx;
  color: #ff1f1f;
}
.navview view.hotelprice text{
  font-size: 36rpx;
  font-family: 'DIN';
}
.navview text.hotelprice{
  font-size: 28rpx;
  color: #333;
  direction: inherit; 
  text-decoration:line-through;
}
.navview text.hotelprice text{
  font-size: 32rpx;
  font-family: 'DIN';
}
.navview .hotelname{
  width: 100%;
  height:54rpx;
  line-height: 54rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 32rpx;
  font-weight: bold;
  color: #000;
}
.navview .distance{
  display: inline-block;
  height: 42rpx;
  line-height: 42rpx;
  margin-top: 6rpx;
  font-size: 30rpx;
  color: #666;
  padding: 0 20rpx; 
  border-left: solid 1rpx #e5e5e5;
}
.navview .distance:first-child{
  border-left:none;
  padding-left: 0;
}
.navview .serve{
  display: inline-block;
  height: 42rpx;
  line-height:42rpx;
  border-radius: 6rpx;
  padding: 0 20rpx;
  margin-top: 6rpx; 
  background: #ffe9cd;
  color: #db8725;
  margin-right: 20rpx;
}
.maproom{
  height: 60rpx;
  line-height: 60rpx;
  float: right;
  font-size: 32rpx;
  color: #666;
  font-weight: bold;
}
.maproom image{
  width: 25rpx;
  height: 30rpx;
  padding: 17rpx 10rpx 8px 0;
  vertical-align: top;
}
.hidetext{
  height:680rpx;
  overflow: hidden;
}
.showtext{
  height:auto;
}
.checkmore{
  clear: both;
  width: 94%;
  margin: 0 auto;
  height: 60rpx;
  line-height:60rpx;
  text-align: right;
  color: #0066eb;
  font-size: 30rpx; 
}