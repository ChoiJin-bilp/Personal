}
.text-box{
  display: inline-block;
  width: 100%;
  height: 180rpx;
  border: solid 2rpx #f5f5f5;
  box-sizing: border-box;
  border-radius: 16rpx;
  margin: 10rpx auto;
  margin-bottom: 0;
  line-height: 40rpx;
  overflow: hidden;
  overflow-y: scroll;
  word-wrap:break-word;
  text-align: justify;
  font-size: 28rpx;
}

