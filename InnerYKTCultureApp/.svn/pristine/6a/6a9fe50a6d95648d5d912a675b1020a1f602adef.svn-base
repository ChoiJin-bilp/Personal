}

.save{
  width: 100%;
  background: #fff;
  text-align: center;padding-bottom: 20rpx;
}
.save button{
  margin-left: 40rpx;
  margin-top: 20rpx;
}
.save button:first-child{
  margin-left: 0;
}
.btn-foot{
  display: inline-block;
  height: 60rpx;
  line-height: 56rpx;
  font-size: 0.75rem;
  box-sizing: border-box;
  border-radius: 60rpx;
  background: #fff;
  padding: 0 60rpx;
