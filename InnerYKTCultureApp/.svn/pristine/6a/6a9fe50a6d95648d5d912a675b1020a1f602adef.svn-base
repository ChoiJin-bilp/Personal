/* .otherbox .perfect-list,.maintipsbox{ */
.otherbox .perfect-list{
  display: inline-block;
  width: 90%;
  margin: 0 auto;
  line-height: 68rpx;
  padding: 10rpx 0;
  margin-left: 5%;
  clear: both;
  position: relative;
  border-bottom: solid 2rpx #f5f5f5;
}
.otherbox .perfect-list text.fleft{
  width: 18%;
  height: 68rpx;
  line-height: 68rpx;
  margin-right: 2%;
  color: #999;
  font-weight:500;
  font-size: 28rpx;
  text-align:justify;
}
.otherbox .perfect-list text.fleft::after{
  width: 100%;
  display: inline-block;
  content: '';
}
.otherbox .perfect-list input{
  width: 78%;
  margin-left: 2%;
  color: #333;
  height: 68rpx;
  line-height: 68rpx;
  font-size: 28rpx;
}
