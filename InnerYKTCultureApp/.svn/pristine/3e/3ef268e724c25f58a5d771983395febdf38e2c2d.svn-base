/* pages/facility/facility.wxss */
page{
  background: #f2f2f2;
  padding: 20rpx;
  box-sizing: border-box;
  font-weight: normal;
}
/*第一模块*/
.facility-o{
  display: flex;
  justify-content: space-between;
}
.facility-o-v{
 display: flex;
 justify-content: space-between;
 align-items: center;
 border: 2rpx solid #ccc;
 padding: 5rpx 20rpx;
 box-sizing: border-box;
 border-radius:5rpx; 
 height: 58rpx;
 width: 70%;
}
.facility-o-v image{
  width: 40rpx;
  height: 40rpx;
}
.facility-o-t{
  background: #E5E5E5;
  height: 58rpx;
  line-height: 58rpx;
  padding: 0 20rpx;
  border-radius: 10rpx;
  color: #333;
}
/*第二模块*/
.facility-t{
  display: flex;
  justify-content: space-between;
  padding: 10rpx;
  box-sizing: border-box;
  position: relative;
  margin-top: 10rpx;
}
.facility-t image{
  width: 50rpx;
  height: 50rpx;
}
.facility-t-v{
  width: 75%;
  display: flex;
  overflow-x: auto;
}
.facility-t-v view{
  flex: 1 0 30%;
  color: #999;
  position: relative;
  font-size: 28rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.facility-t-v view text{
  box-sizing: border-box; 
  text-align: center;
}
.facility-t-v image{
 width: 25rpx;
 height: 25rpx;
}
.posty{
  z-index: 10;
 position: absolute;
 top: -3%;
  right: 0%;
}
.svsit{
  width: 20%;
 display: flex;
 justify-content: space-between;
  align-items: center;
}
.svsit text{
  color: #0165E7;
}
.svsit image{
  width: 40rpx;
  height: 40rpx;
}
.dance{
  z-index: 20;
  position: absolute;
  height: 100%;
  width: 23rpx;
  top: 0%;
  right: 25%;
  background: #f2f2f2;
  opacity: 0.8;
}

.partner{
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin: 20rpx 0;
}
.partner text{
  width: 25%;
  color: #ccc;
}

.facility-s{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
  font-size: 30rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
}
.facility-s-o{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.anniu{
  border: #999 1rpx solid;
  padding: 0 30rpx;
  box-sizing: border-box;
  border-radius: 40rpx;
  width: 122rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #747474;
  margin-right: 20rpx;
}
.onest{
  width:96%;
  display: flex;
  align-items: unset;
}
.onest .onest-t{
  display: inline-block;
  color: #999;
  width: 26%;
  white-space: nowrap;
}
