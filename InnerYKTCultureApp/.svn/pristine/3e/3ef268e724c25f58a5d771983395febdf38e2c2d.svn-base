.facility-s-s{
  display: flex;
  justify-content: space-between;
}
.blu{
  width: 65%;
  display: flex;
  justify-content: flex-end;
}
.discolor{
  color: #000;
  border-bottom: 1rpx solid #000;
}


.tanChuan{
  z-index: 200;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #7B7B7B;
  opacity: 0.6;
}
.tanChuan-v{
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 92%;
  height: 450rpx;
  background: #fff;
  border-radius: 20rpx;
  padding-bottom:58rpx;
  top:50%;
  left:50%;
  transform: translateX(-50%) translateY(-50%);
  position: fixed;z-index: 9999;
}
.tanChuan-v view{
  margin-top: 40rpx;
}
.tanChuan-v-o{
  width: 100%;
  text-align: center;
  font-size: 38rpx;
  font-weight: bold;
}
.tanChuan-v-t{
  font-size: 36rpx;
  width: 100%;
  padding: 0 60rpx;
  box-sizing: border-box;
}
.tanChuan-v-t input{
  padding: 0 20rpx;
  height: 50rpx;
  box-sizing: border-box;
  border: #999 solid 1rpx;
}
.tanChuan-v-s{
  display: flex;
  width: 100%;
  justify-content: space-around;
  align-items: center;
}
.tanChuan-v-s view{
  font-size: 38rpx;
  padding: 0 20rpx;
  width: 25%;
  line-height: 70rpx;
  border-radius: 10rpx;
  text-align: center;
  border: 2rpx solid #999;
}
.option{
  display: flex;
}
.choice{
  display: flex;
  justify-content: center;
  align-items: center;
}
.choice text{
  font-size: 32rpx;
}
.diot{
  margin: 0 10rpx;
  width: 30rpx;
  height: 30rpx;
  border: #ccc solid 1rpx;
  position: relative;
  border-radius:50%; 
}
.huicolor{
  /* background: #26E954; */
  background: #FC5B04;
}
.huicolortt{
   background: #26E954; 
}
