.adelnum {
  width: 42rpx;
  height: 42rpx;
  border: #2f9f82 solid 2rpx;
  border-radius: 50%;
  color: #2f9f82;
  padding-bottom: 21rpx;
  box-sizing: border-box;
  position: relative;
}

.adelnum::after {
  content: '';
  position: absolute;
  border: solid #2f9f82 2rpx;
  width: 21rpx;
  top: 48%;
  left: 50%;
  transform: translateX(-50%);
}

.addnum {
  width: 42rpx;
  height: 42rpx;
  /* border: #2F9F82 solid 4rpx; */
  background: #2f9f82;
  border-radius: 50%;
  font-size: 42rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  box-sizing: border-box;
  position: relative;
}

.addnum::after {
  content: "";
  width: 50%;
  border: 1rpx #fff solid;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%) rotate(90deg);
}

.addnum::before {
  content: "";
  width: 50%;
  border: 1rpx #fff solid;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%) rotate(0deg);
}

