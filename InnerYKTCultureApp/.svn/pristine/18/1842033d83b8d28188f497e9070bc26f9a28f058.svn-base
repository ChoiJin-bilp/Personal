  box-sizing: border-box;
  animation:loosh  1.5s cubic-bezier(1, 1, 1, 1) infinite;
}
@keyframes loosh {
  0% {
    transform: scale(1);
  }
  50%{
    transform: scale(1.1);
  }
  100% {
    transform: scale(1);
  }
}