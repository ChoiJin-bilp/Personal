page{
  background: #f6f6f6;
}
.width94{
  width: 96%;
  background: #fff;
  margin: 20rpx auto;
  border-radius: 16rpx;
  overflow: hidden;

   float: left;
  line-height: 72rpx;
  font-size: 28rpx;
  color: #666;
}
.cart-list{
    position: relative;
     width: 100%;
    padding: 30rpx 20rpx 30rpx 204rpx;
    height: 164rpx;
    border-bottom: 1rpx solid #e9e9e9;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 90rpx;
    width: 44rpx; 
    height: 44rpx;
}

.cart-list .cart-thumb{
    position: absolute;
    top:30rpx;
    left: 20rpx;
    width: 164rpx;
    height: 164rpx;
    border: solid 2rpx #f8f8f8;
    box-sizing: border-box;
    border-radius: 12rpx;
}
.cart-infor{
  width: 500rpx;
  height: 164rpx;
}
.cart-list .cart-pro-name{
  display: inline-block;
  width: 100%;
  height: 72rpx;
  line-height: 36rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
  font-weight: bold;
  color: #000;
}
.cart-norms{
  width: 100%;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 30rpx;
  color: #333;
}
.cart-moeny{
  width: 100%;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 30rpx;
  color: #333;
}
.cart-list .cart-pro-price{
    display: inline-block;
    color: #ff1f1f;
    font-size: 24rpx;
}
.cart-list .cart-pro-price text{
    font-size: 32rpx;
    font-weight: bold;
}
.cart-list .cart-count-box{
    float: right;
    width: 220rpx;
    height: 46rpx;
}
.cart-list .cart-count-box text{
    display: inline-block;
    vertical-align: middle;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
}
.cart-count-down,.cart-count-add{
    font-size: 44rpx;
    width: 46rpx;
    line-height: 40rpx!important;
    border: solid 2rpx #e9e9e9;
}
.cart-count-num{
    width: 120rpx;
    border-top: solid 2rpx #e9e9e9;
    border-bottom: solid 2rpx #e9e9e9;
   
}
.cart-count-down{
  border-top-left-radius: 12rpx;
  border-bottom-left-radius:12rpx;
}
.cart-count-add{
  border-top-right-radius: 12rpx;
  border-bottom-right-radius:12rpx;
}
.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}

.orderdetails{
  width: 96%;
  height: 72rpx;
  clear: both;
  margin: 0 auto;
}
.orderdetails text{
  float: left;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 28rpx;
  color: #666;
}
.order-btn{
  float: right;
  height: 72rpx;
}
.order-btn button{
  display: inline-block;
  background: #fff;
  height: 52rpx;
  line-height: 48rpx;
  margin-top: 10rpx;
  border: solid 2rpx #136fe6;
  box-sizing: border-box;
  font-size: 28rpx;
  padding: 0 26rpx;
  border-radius: 26rpx;
  color: #136fe6;
  margin-left: 20rpx;
}
.order-btn button:first-child{
  margin-left: 0;
}

.savebox1{
  width:90%;
  position: relative;
  top:50%;left:50%;
  transform: translate(-50%,-70%);
  z-index: 9999;
  max-height: 80%;
  border-radius: 8px;
  overflow: hidden;
  padding-bottom: 110rpx;
}
.pop-array1{
  width: 96%;
  height: 96%;
  background: #fff;
  border-radius: 16rpx;
  padding: 2%;
}
.SP-infor{
  display: block;
  width: 100%;
  height: 60rpx;
  font-size: 26rpx;
  position: relative;
}
.SP-infor text{
  display: inline-block;
  color: #000;
  max-width: 400rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis; 
  height: 60rpx;
  line-height: 60rpx;
}
.SP-infor text:first-child{
  display: inline-block;
  color: #666; 
  height: 60rpx;
  line-height: 60rpx;
}
.SP-infor view{
  position: absolute;
  right: 0;
  top: 0;
  color: #136fe6;
  height: 60rpx;
  line-height: 60rpx;
}
.delete-box{
  width:100%;
  height:100rpx;
  position: absolute;
  bottom:0;left:0;
  z-index: 99999;
  text-align: center;
}
.del-record{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  margin-top: 20rpx;
}
.savebox{
  width:86%;
  position: absolute;
  top:50%;left:50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
  border-radius: 8px;
  background: #fff;
  overflow: hidden;
}
