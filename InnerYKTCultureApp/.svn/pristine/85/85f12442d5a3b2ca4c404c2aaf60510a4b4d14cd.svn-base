/*优惠劵样式*/
.swiper {
  width: 100%;
  height: 590rpx;
}
swiper-item {
  overflow-y: scroll;
}

.userule{
  position: fixed;
  left: 0;top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
}
.bgopcity{
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.4;
  position: absolute;
  left: 0; top: 0;
  z-index: 1050;
}
.textrule{
  width: 80%;height: 80%;
  position: absolute;
  left: 10%;
  top: 10%;
  background: #fff;
  z-index: 9999;
  border-radius:8px;
  color: #999;
  font-size: 28rpx; 
  overflow-y: scroll;
}
.textrule text{
  display: block;
  width: 96%;
  line-height: 20px;
  margin: 4px auto;
  text-align: justify;
}
.textrule view{
  width: 100%;
  height: 40px;
  line-height: 40px;
  color: #333;
  font-size: 32rpx;
  text-align: center;
  font-weight: 400;
}
/*优惠劵*/
.Acoupon{
  position: relative;
  width: 690rpx;
  height: 242rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  box-shadow:0px 4rpx 6rpx rgba(221, 221, 226, 0.6);
  border-radius: 12rpx;
  overflow: hidden;
}
.shadow{
  width: 690rpx;
  height: 244rpx;
  background: #000;
  opacity: 0;
  position: absolute;
  left: 0;top: 0;
  z-index: 105;
}
.shadow1{
  width: 690rpx;
  height: 244rpx;
  background: #000;
  opacity: 0.3;
  position: absolute;
  left: 0;top: 0;
  z-index: 105;
}
.leftradius{
   position: absolute;
  width: 28rpx;
  height: 28rpx;
  background: #f6f6f6;
  left: -14rpx;
  top: 150rpx;
  z-index: 110;
  border-radius: 50%;
}
.rightradius{
   position: absolute;
  width: 28rpx;
  height: 28rpx;
  background: #f6f6f6;
  right: -14rpx;
  top: 150rpx;
  z-index: 110;
  border-radius: 50%;
}
.Acoupon image{
  position: absolute;
  left: 0; top: 0;
  width: 690rpx;
  height: 242rpx;
  z-index: 99;
}
.Acouponinfor{
  position: absolute;
