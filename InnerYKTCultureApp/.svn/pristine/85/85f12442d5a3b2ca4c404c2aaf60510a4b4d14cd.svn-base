/* packageVP/pages/MywantMassage/MywantMassage.wxss */
page{
  padding: 30rpx;
  box-sizing: border-box;
  padding-bottom: 150rpx;
}
.introduce{
  width: 100%;
  display: flex;
  justify-content: space-around;
  color: #3A97EE;
  font-size: 32rpx;
  align-items: center;
}
.introduce view{
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.introduce view text{
  margin-left: 6rpx;
}
.osize{
  width:36rpx;height:36rpx;
}
.tanChuan{
  z-index: 200;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.7;
  pointer-events: all;
}
.tanChuan-v{
  padding: 40rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 92%;
  background: #fff;
  border-radius: 20rpx;
  padding-bottom:58rpx;
  padding-top:0rpx;
  top:50%;
  left:50%;
  transform: translateX(-50%) translateY(-55%);
  position: fixed;z-index: 250;
}
.tanChuan-v-t{
  font-size: 36rpx;
  width: 100%;
  padding:0;
  max-height: 800rpx;
  overflow-y: auto;
  box-sizing: border-box;
}
.close{
  position: absolute;
  bottom: 80rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 250rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background: #fff;
  color: #000;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 30rpx auto;
  font-size: 60rpx;
}
.close-o{
  position: absolute;
  left: 50%;
  top: 50%;
  width: 60%;
  border-bottom: rgb(134, 99, 99) solid 4rpx;
  transform: translateX(-50%) translateY(-50%) rotate(45deg);
}
.close-t{
  position: absolute;
  left: 50%;
  top: 50%;
  width: 60%;
  border-bottom: rgb(134, 99, 99) solid 4rpx;
  transform: translateX(-50%) translateY(-50%) rotate(135deg); 
}
.title{
  width: 100%;
  text-align: center;
  font-size: 34rpx;
  color: #333;
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  font-weight: bold;
  margin-top: 40rpx;
  margin-bottom: 30rpx;
}
.struction{
  box-sizing: border-box;
  width: 100%;
}
.content{
  text-indent: 1rem;
  color: #666;
  line-height: 48rpx;
  font-size: 30rpx;
}
.roof{
  margin-top: 20rpx;
}
.tryto{
  font-size: 32rpx;
color: #333333;
line-height: 60rpx;
font-weight: bold;
}
.ourep{
  margin-top: 20rpx;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  background: linear-gradient(-30deg,#3a97ee 0%, #4a6fdc 100%);
border-radius: 10rpx;
display: flex;
justify-content: space-between;
align-items: center;
min-height: 230rpx;
}
.ourepimg{
  width: 12rpx;
  height: 20rpx;
  transform: scale(1.5);
}
.plirted{
display: flex;
align-items: center;
}
.plirtedimg{
width: 140rpx;
height: 140rpx;
margin-right: 30rpx;
}
.plirted-z view{
  font-size: 34rpx;
color: #fefefe;
line-height: 60rpx;
}
.plirted-z view:nth-of-type(2){
  opacity: 0.7;
  font-size: 28rpx;
  line-height: 40rpx;
  margin-top:10rpx;
}
/*按摩劵样式*/

.massageSecurities {
  width: 100%;
  box-sizing: border-box;
  height: 700rpx;
  overflow-y: auto;
}
.massageSecurities2{
  width: 100%;
  box-sizing: border-box;
  height: 700rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color:#999;
  font-size: 36rpx;
}
.massgelist {
  position: relative;
  height: 210rpx;
}

.massgelist image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.mainmessage {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
  display: flex;
}

.massageOne {
  width: 67%;
  padding: 14rpx 20rpx;
  box-sizing: border-box;
  color: #fff;
  font-size: 34rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}

.useTime {
  display: flex;
  align-items: flex-end;
}

.TimeFont {
  font-size: 82rpx;
  line-height: 82rpx;
}

.Timeunit {
  margin-left: 10rpx;
  display: flex;
  align-items: flex-end;
}

.massageSecond {
  width: 33%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.massageSecond .startAM {
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 62rpx;
  width: 206rpx;
  border: #fff solid 2rpx;
  border-radius: 50rpx;
  font-size: 40rpx;
}

.massageSecond .startAM1 {
  background: #C8C8C8;
  color: #999;
  border: #FFF solid 2rpx;
}

.stare {
  margin-top: 10rpx;
  display: flex;
  width: 90%;
  color: #fff;
  font-size: 30rpx;
  align-items: center;
  background: transparent;
  padding-left: 20rpx;
  box-sizing: border-box;
}

.stare image {
  width: 38rpx;
  height: 38rpx;
  position: relative;
  margin-right: 10rpx;
}
.stare2{
  width: 130%;
  text-align: center;
}
