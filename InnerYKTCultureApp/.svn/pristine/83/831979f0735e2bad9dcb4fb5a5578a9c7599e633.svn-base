/* pages/conversionorder/conversionorder.wxss */
page{
  background: #f6f6f6;
}
.cart-box{
    padding-bottom: 124rpx;
    padding-top: 120rpx;
}
.cart-list{
    /* position: relative; */
    width: 94%;
    height:320rpx;
    background: #fff;
    margin: 20rpx auto;
    border-radius: 16rpx;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 90rpx;
    width: 44rpx; 
    height: 44rpx;
}
.shopdetails{
  width: 94%;
  height: 164rpx;
  margin: 0 auto;
}
.cart-list .cart-thumb{
    float: left;
    width: 164rpx;
    height: 164rpx;
    border: solid 2rpx #f8f8f8;
    box-sizing: border-box;
    border-radius: 12rpx;
}
.cart-infor{
  float: right;
  width: 490rpx;
  height: 164rpx;
}
.cart-list .cart-pro-name{
  display: inline-block;
  width: 100%;
  height: 72rpx;
  line-height: 36rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
  font-weight: bold;
  color: #000;
}
.cart-norms{
  width: 100%;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 30rpx;
  color: #333;
}
.cart-moeny{
  width: 100%;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 30rpx;
  color: #333;
}
.cart-list .cart-pro-price{
    display: inline-block;
    color: #ff1f1f;
    font-size: 24rpx;
}
.cart-list .cart-pro-price text{
    font-size: 32rpx;
    font-weight: bold;
}
.cart-list .cart-count-box{
    float: right;
    width: 220rpx;
    height: 46rpx;
}
.cart-list .cart-count-box text{
    display: inline-block;
    vertical-align: middle;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
}
.cart-count-down,.cart-count-add{
    font-size: 44rpx;
    width: 46rpx;
    line-height: 40rpx!important;
    border: solid 2rpx #e9e9e9;
}
.cart-count-num{
    width: 120rpx;
    border-top: solid 2rpx #e9e9e9;
    border-bottom: solid 2rpx #e9e9e9;
   
}
.cart-count-down{
  border-top-left-radius: 12rpx;
  border-bottom-left-radius:12rpx;
}
.cart-count-add{
  border-top-right-radius: 12rpx;
  border-bottom-right-radius:12rpx;
}
.cart-header{
    position: fixed;
    top: 0rpx;
    left: 3%;
    width: 94%;
    height: 120rpx;
    padding:0 60rpx 0 80rpx;
    box-sizing: border-box;
    background: #fff;
    z-index: 999;
    border-radius: 16rpx;
  
}

.total-select{
    position: absolute;
    left: 20rpx;
    top: 10rpx;
    width: 44rpx;
    height: 44rpx;
    vertical-align: middle;
}
.order-icon{
    position: absolute;
    right: 40rpx;
    top: 25rpx;
    width: 44rpx; 
    height: 44rpx;
}
.order-icon image,.order-icon navigator{
  display: block;
  width: 44rpx; 
  height: 44rpx;
}
.cart-toatl-price{
    float: right;
    width: 120rpx;
}

.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}

.cart-footer{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 96%;
    height: 120rpx;
    padding:0 2%;
    background: #eee;
}
.shoppay{
  width: 320rpx;
  float: left;
  height: 80rpx;
  margin:20rpx; 
}
.total{
  width: 100%;
  height: 40rpx;
  line-height: 36rpx;
  font-size: 30rpx;
  color: #000;
  margin-top: 20rpx;
}
.total text{
  font-size: 36rpx;
  color: #ff1f1f;
  font-weight: bold;
}
.total text text{
  font-size: 28rpx;
  color: #666;
  margin-left: 20rpx;
}
.privilege{
  width: 100%;
  height: 40rpx;
  line-height: 44rpx;
  font-size: 30rpx;
  color: #666;
}
.privilege text{
  font-size: 24rpx;
  color: #666;

}
.now-pay{
  display: inline-block;
  width: 220rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #fff;  
  text-align: center;
  background: -webkit-linear-gradient(left, #ff1f1f , #ff592d); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #ff1f1f, #ff592d); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #ff1f1f, #ff592d); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #ff1f1f , #ff592d); /* 标准的语法（必须放在最后） */ 
  border-radius: 40rpx;
   margin: 20rpx;
}
.cart-header image.fleft{
  width: 38rpx;
  height: 46rpx;
  position: absolute;
  left: 20rpx;
  top:37rpx;
  vertical-align: middle;
}
.cart-header image.fright{
  width: 16rpx;
  height: 26rpx;
  position: absolute;
  right: 20rpx;
  top:47rpx;
  vertical-align: middle;
}
.recipients{
  width: 100%;
  height: 80rpx;
  overflow:hidden;
  margin-top: 20rpx;
}
.use-order{
  width: 100%;
  height: 40rpx;
  line-height:40rpx;
  font-size: 28rpx;
  color: #333; 
  /* font-weight: bold;  */
}
.use-order text{
  font-size: 28rpx;
  color: #666;
  font-weight: normal;
  margin-left: 20rpx;
}
.use-address{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 26rpx;
  color: #333;
}
.use-YH{
  width: 100%;
}
.use-privilege{
  width: 100%;
  padding: 0 30rpx 0 30rpx;
  height:72rpx;
  line-height: 72rpx;
  position: relative;
  clear: both;
  background: #f8f8f8;
  border-bottom: solid 2rpx #e9e9e9; 
}
.tips-title{
  font-size: 28rpx;
  color: #000;
}
.choose{
  float: right;
  display: block;
  font-size: 28rpx;
  position: absolute;
  right:80rpx; 
  top: 0;
  color: #ff1f1f;
}
.choose image{
  height: 16rpx;
  width: 26rpx;
  margin: 0 0 0 10rpx;
  vertical-align: middle;
}
.use-show-privilege{
  width: 100%;
  position: relative;
  transition: height 0.3s;
  display: flex;
  /*row 横向  column 列表  */
  flex-direction: column;
  align-items:center; 

}
.use-show-privilege text{
  display: inline-block;
  width: 94%;
  height: 72rpx;
  line-height: 72rpx;
  border-bottom: solid 2rpx #e9e9e9;
  text-align: right;
  padding:0 3% 0 3%;
  background: #f8f8f8;
  font-size: 28rpx;
  color: #333;
}
.ashopname{
  padding:10rpx 3%;
}
.storenumber{
  width: 94%;
  margin: 0 auto;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: #999;
  padding-top: 20rpx;

}