.fonts{
position: relative;
}
.fonts::after{
  content: "";
  width: 0rpx;
  height: 0rpx;
  border-bottom: 30rpx solid transparent;
  border-top: 42rpx solid #FFF;
  border-left: 0rpx solid transparent;
  border-right: 60rpx solid transparent;
  position: absolute;
  left: -26rpx;
  bottom: -82rpx;
  animation: showZeroAlert 1s ease infinite alternate;
  z-index: 2;
}
.fonts::before{
  content: "";
  width: 0rpx;
  height: 0rpx;
  border-bottom: 30rpx solid transparent;
  border-top: 44rpx solid #999;
  border-left: 0rpx solid transparent;
  border-right: 60rpx solid transparent;
  position: absolute;
  left: -28rpx;
  bottom: -86rpx;
  animation: showZeroAlert 1s ease infinite alternate;
  z-index: 0;
}
