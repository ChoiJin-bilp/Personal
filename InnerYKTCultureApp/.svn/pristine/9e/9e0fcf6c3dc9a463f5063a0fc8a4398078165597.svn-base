/* packageYK/pages/GetAMassage/GetAMassage.wxss */
.Lotterypop{
  width:92%;
  margin: 0rpx auto;
  margin-top: 70rpx; 
  border-radius: 8px;
  padding:26rpx;
  max-height: 730rpx;
  box-sizing: border-box;
  background: -webkit-linear-gradient(top, #ed6a32, #db2528); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(bottom, #ed6a32, #db2528); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(bottom, #ed6a32, #db2528); /* Firefox 3.6 - 15 */
  background: linear-gradient(to bottom, #ed6a32, #db2528); /* 标准的语法（必须放在最后） */
  overflow-y: auto;
}
.Lotterypop .Lotteryscrollview{
  width: 100%;
  padding-bottom: 20rpx;
  height: 600rpx;
  max-height: 600rpx;
  box-sizing:border-box;
  overflow: hidden;
  overflow-y: scroll;
}
.titleview1{
  width: 100%;
  font-size: 30rpx;
  color: #f9d7d0;
  padding: 10rpx 0 10rpx 0;
  text-align: center;
}
.mywinview{
  width: 100%;
  padding: 20rpx;
  height: 120rpx;
  line-height: 34rpx;
  background: #fff1c2;
  border-radius: 12rpx;
  margin-top: 30rpx;
  text-align: center;
  font-size: 32rpx;
  color: #8c5443;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}
.mywinview text{
  padding: 0 6rpx;
  font-size: 48rpx;
}
.nowaway{
  display: inline-block;
  width: 60%;
  padding: 0;
  height: 78rpx;
  font-size: 40rpx;
  line-height: 80rpx;
  border-radius: 30rpx;
  text-align: center;
  margin: 40rpx 20% 0 20%;
  color: #dd2b29;
  background: -webkit-linear-gradient(top, #fff5d1, #fbb25e); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(bottom, #fff5d1, #fbb25e); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(bottom, #fff5d1, #fbb25e); /* Firefox 3.6 - 15 */
  background: linear-gradient(to bottom, #fff5d1, #fbb25e); /* 标准的语法（必须放在最后） */
  /* font-weight: bold; */
  animation: Flow 0.7s ease infinite alternate;
}
.tip{
  margin: 0;
  color: #fff;
  font-size: 26rpx;
}
.colorlist{
  animation: Flow 0.7s ease infinite alternate;
}
@keyframes Flow {
    from {
        /* text-shadow: 0 0 10rpx #fff,
                     0 0 20rpx #fff,
                     0 0 30rpx #fff, */
        transform: scale(1);
    }
    to {
        /* text-shadow: 0 0 10rpx #E2DC64,
                     0 0 20rpx #E2DC64,
                     0 0 30rpx #E2DC64; */
        transform: scale(1.1);

    }
}
/*规则css*/
.ticket{
    width: 700rpx;
    margin: 10rpx auto;
    padding: 20rpx;
    box-sizing: border-box;
    /* background: #FFBCA0; */
    background: #e5e5e5;
    border-radius:10rpx; 
    margin-top: 100rpx;
    height: 350rpx;
    /* position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: 0; */
}
.ticket-v{
  display: flex;
  justify-content: space-between;
}
.ticket-v view:nth-of-type(1){
  color: #333;
  font-weight: bold;
  line-height: 50rpx;
  border-bottom:#333 solid 3rpx; 
}
.ticket-v view:nth-of-type(2){
  font-weight: normal;
}
.ticket-o{
  font-size: 28rpx;
  margin-top:20rpx;
  line-height: 40rpx; 
  color: #333;
}










