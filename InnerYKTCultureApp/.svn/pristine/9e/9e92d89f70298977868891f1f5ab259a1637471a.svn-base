/* pages/anmojuan/anmojuan.wxss */
page{
  height:100%;
  padding-top: 72rpx;
}
/*顶部样式*/

.topTabSwiper {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  background: #eee;
  height: 72rpx;
  line-height: 72rpx;
  z-index: 150;
  /* display: flex;
  justify-content: center;
  align-items: center; */
  overflow-x: auto;
}

.topTabSwiper:after {
  content: "";
  clear: both;
  display: block;
}

.tabbox {
  width: 25%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  float: left;
  font-size: 28rpx;
}

.tab {
  display: inline-block;
  height: 70rpx;
  line-height: 70rpx;
  box-sizing: border-box;
}

.tab:last-child {
  border-right: 0;
}

.tabBorer {
  border-bottom: 2px solid #0066eb;
  color: #0066eb;
  box-sizing: border-box;
}

/*按摩劵样式*/

.massageSecurities {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}

.massgelist {
  position: relative;
  min-height: 210rpx;
}

.massgelist image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.mainmessage {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
  display: flex;
}

.massageOne {
  width: 67%;
  padding: 14rpx 20rpx;
  box-sizing: border-box;
  color: #fff;
  font-size: 34rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}

.useTime {
  display: flex;
  align-items: flex-end;
}

.TimeFont {
  font-size: 82rpx;
  line-height: 82rpx;
}

.TimeFont2 {
  font-size: 60rpx;
  line-height: 60rpx;
  font-family: DIN;
}

.Timeunit {
  margin-left: 10rpx;
  display: flex;
  align-items: flex-end;
}

.massageSecond {
  width: 33%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.massageSecond .startAM {
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 62rpx;
  width: 206rpx;
  border: #fff solid 2rpx;
  border-radius: 50rpx;
  font-size: 40rpx;
}

.massageSecond .startAM1 {
  background: #C8C8C8;
  color: #999;
  border: #FFF solid 2rpx;
}

.massageSecond .startAM2 {
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 50rpx;
  width: 206rpx;
  border: #fff solid 2rpx;
  border-radius: 50rpx;
  font-size: 28rpx;
  background: transparent;
}
.massageSecond .startAM2:nth-of-type(1){
  margin-bottom: 30rpx;
}

.stare {
  margin-top: 10rpx;
  display: flex;
  width: 90%;
  color: #fff;
  font-size: 30rpx;
  align-items: center;
  background: transparent;
  padding-left: 20rpx;
  box-sizing: border-box;
}

.stare image {
  width: 38rpx;
  height: 38rpx;
  position: relative;
  margin-right: 10rpx;
}
.stare2{
  width: 130%;
  text-align: center;
}
.TimeAM {
  margin: 6rpx 0;
}
.TimeAM2 {
font-size: 28rpx;
color: #ffffff;
}

.Timeunit .price {
  background: #C8C8C8;
  padding: 4rpx 10rpx;
  color: #fff;
  font-size: 34rpx;
  border-radius: 10rpx;
  margin-left: 10rpx;
}
.Timeunit .outfont{
  font-size: 30rpx;
  color: #ff3b3b;
  margin-left: 10rpx;
}
.moptdata{
  font-size: 24rpx;
  color:#FFF;
  display: flex;
}
.moptloe{
  padding: 4rpx 10rpx;
  background:rgb(255, 255, 255,0.6);
  border-radius: 4rpx;
  color:#FA612B;
}
/*固定阴影弹出样式*/

.windowBackgroud {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgb(7, 7, 7, 0.6);
  z-index: 120;
}

/*自定义弹窗css*/

.windowitem {
  position: fixed;
  bottom: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(50%);
  z-index: 125;
  width: 94%;
  background: #eff0f2;
  border-radius:20rpx; 
}

.Lotterydel {
  position: absolute;
  right: -320rpx;
  top: -100rpx;
  z-index: 55;
  width: 100%;
  height: 80rpx;
  text-align: center;
  opacity: 0.3;
}

.Lotterydel image {
  width: 60rpx;
  height: 60rpx;
  padding: 20rpx 0 0 0;
}
.payTitle{
  color: #000;
  font-weight: bold;
  margin-bottom: 40rpx;
  font-size: 38rpx;
  
}
.productItem{

}
.choice{
  display: flex;
  align-items: center;
  justify-content: center;
}
.diot{
  margin: 0 10rpx;
  width: 38rpx;
  height: 38rpx;
  border: #ccc solid 1rpx;
  position: relative;
  border-radius:50%; 
}
.all{
  border: #0065EB solid 1rpx;
}
.all::after{
  content: "";
  position: absolute;
  background: #0065EB;
  top: 50%;
  left: 50%;
  width: 18rpx;
  height: 18rpx;
  border-radius:50%; 
  transform: translateX(-50%) translateY(-50%);
}
.chioceTime{
  width: 560rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 40rpx;
  box-sizing: border-box;
  border: #CFCFCF 2rpx solid;
  border-radius:14rpx; 
  margin-left: 20rpx;
  font-size: 38rpx;
  font-weight: bold;
}
.chioceTime image{
  height: 115rpx;
  width: 72rpx;
}
.chioceBorder{
    border: #0066FF 2rpx solid;
}
.chioceFont{
  color: #0066FF;
}
.annotation{
  width: 100%;
  font-size: 30rpx;
  text-align: left;
  line-height: 50rpx;
  color: #333;
  margin-top: 20rpx;
  margin-bottom: 50rpx;
}
.annotation view:nth-of-type(1){
  font-size: 28rpx;
  color: #FF3333;
}
.payBut{
 color: rgb(46, 102, 223);
 height: 60rpx;
 width: 380rpx;
 border:rgb(46, 102, 223) 2rpx solid;
 display: flex;
 align-items: center;
 justify-content: center; 
 margin: 0 auto;
border-radius: 40rpx;

}
.payBut text:nth-of-type(1){
  margin-right: 24rpx;
}
.window{
  padding:30rpx 20rpx; box-sizing: border-box; text-align:center;width:100%;height:100%;position: relative;
}


