@keyframes Flow {
  from {
    color: #fff;
    /* text-shadow: 0 0 10rpx #fff,
                     0 0 20rpx #fff,
                     0 0 30rpx #fff, */
    transform: scale(1);
  }

  to {
    /* color:#EE721D; */
    color: #ff0;
    /* text-shadow: 0 0 10rpx #E2DC64,
                     0 0 20rpx #E2DC64,
                     0 0 30rpx #E2DC64; */
    transform: scale(1.1);
  }
}

.tefont {
  -webkit-transition: all 1.5s ease;
  transition: all 1.5s ease;
  -webkit-animation: Glow 1.5s ease infinite alternate;
  animation: Glow 1.5s ease infinite alternate;
}

@-webkit-keyframes Glow {
  from {
    text-shadow: 0 0 10rpx #fff,
                     0 0 20rpx #fff,
                     0 0 30rpx #fff,
                     0 0 40rpx #00a67c,
                     0 0 70rpx #00a67c,
                     0 0 80rpx #00a67c,
                     0 0 100rpx #00a67c,
                     0 0 150rpx #00a67c;
  }

  to {
    text-shadow: 0 0 5rpx #fff,
                     0 0 10rpx #fff,
                     0 0 15rpx #fff,
                     0 0 20rpx #00a67c,
                     0 0 35rpx #00a67c,
                     0 0 40rpx #00a67c,
                     0 0 50rpx #00a67c,
                     0 0 75rpx #00a67c;
  }
}

@keyframes Glow {
  from {
    text-shadow: 0 0 10rpx #fff,
                     0 0 20rpx #fff,
                     0 0 30rpx #fff,
                     0 0 40rpx #00a67c,
                     0 0 70rpx #00a67c,
                     0 0 80rpx #00a67c,
                     0 0 100rpx #00a67c,
                     0 0 150rpx #00a67c;
  }

  to {
    text-shadow: 0 0 5rpx #fff,
                     0 0 10rpx #fff,
                     0 0 15rpx #fff,
                     0 0 20rpx #00a67c,
                     0 0 35rpx #00a67c,
                     0 0 40rpx #00a67c,
                     0 0 50rpx #00a67c,
                     0 0 75rpx #00a67c;
  }
}

