}

.close {
  position: absolute;
  bottom: 34rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 250rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background: #fff;
  color: #000;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 30rpx auto;
  font-size: 60rpx;
}

.close-o {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 60%;
  border-bottom: rgb(134, 99, 99) solid 4rpx;
  transform: translateX(-50%) translateY(-50%) rotate(45deg);
}

.close-t {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 60%;
  border-bottom: rgb(134, 99, 99) solid 4rpx;
  transform: translateX(-50%) translateY(-50%) rotate(135deg);
}

