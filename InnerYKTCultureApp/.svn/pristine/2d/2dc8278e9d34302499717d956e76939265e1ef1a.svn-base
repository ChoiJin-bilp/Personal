}

.tanChuan-av {
  padding: 40rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 92%;
  background: #fff;
  border-radius: 20rpx;
  padding-bottom: 58rpx;
  padding-top: 0rpx;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-55%);
