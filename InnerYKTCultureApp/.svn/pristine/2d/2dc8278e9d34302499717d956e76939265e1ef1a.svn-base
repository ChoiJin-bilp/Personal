@keyframes sploosh {
  0% {
    box-shadow: 0 0 0 0px rgba(15, 141, 233, 0.7);
    background: rgba(15, 141, 233, 0.7);
  }

  100% {
    box-shadow: 0 0px 30rpx 30px rgba(15, 141, 233, 0);
    background: rgba(15, 141, 233, 0);
  }
}

.dot {
  animation: sploosh 1.5s cubic-bezier(1, 1, 1, 1);
}

.dot2 {
  animation: sploosh 1.5s cubic-bezier(1, 1, 1, 1);
}

.dot3 {
  animation: sploosh 1.5s cubic-bezier(1, 1, 1, 1);
}

.dot, .dot2, .dot3 {
  height: 358rpx;
  width: 356rpx;
  font-size: 20px;
  color: #fff;
  border-radius: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateY(-50%) translateX(-50%);
  z-index: -4;
  background: rgba(15, 141, 233, 0.7);
  animation-iteration-count: infinite;
}

