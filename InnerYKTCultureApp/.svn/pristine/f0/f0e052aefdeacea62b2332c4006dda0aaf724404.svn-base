/*新版*/
.tab{
  float: left; 
  width:24%;
  height: 70rpx;
  margin-left: 7%;
  text-align: center;
  box-sizing: border-box;
 
}
.tab:last-child{
  border-right: 0;
}
.topTabSwiper{
  position: fixed;left: 0;top: 0;
  width: 100%;
  z-index: 2;
  background: #fff;
