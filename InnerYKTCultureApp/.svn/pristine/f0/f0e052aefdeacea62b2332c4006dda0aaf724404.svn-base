  line-height: 72rpx;
  
    display: flex;
    justify-content: center;
    align-items: center;
}
.topTabSwiper:after{content: ""; clear: both;display: block;}
.tabBorer{
  border-bottom: 1px solid #0066eb; 
  color: #0066eb;
  box-sizing: border-box;
}
.swiper{width: 100%;position: relative;top: 72rpx;}
.swiper_con{text-align: center; width: 100%; height: 100%;}
swiper-item{overflow-y: scroll; }

.add-phone{
  width: 100%;
  padding-top: 60rpx;
  height: 100%;
  overflow-y: scroll;
  background: #f9f9f9;
  position: relative;
}
.mycard-title{
  position: fixed;
  left: 0; 
  top: 0;
  height: 70rpx;
  padding: 0 4%; 
  line-height: 70rpx;
  font-size: 0.9rem;
  background: #fff;
  z-index: 999;
  text-align: center;
}
.mycard-title image{
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
  padding: 11rpx 20rpx;
}
.mycard-title text{
  display: inline-block;
  height: 48rpx;
  line-height: 48rpx;
  padding: 11rpx 0 11rpx 10rpx;
}
.mycard-title input{
  margin: 10rpx auto;
  background: #f6f6f6;
  height: 60rpx;
  width: 88%;
  padding: 0 3%;
  border-radius: 60rpx;
  font-size:  0.85rem;
}
.mycard{
  height: 120rpx;
  margin: 0 auto;
  border: solid 2rpx #ddd;
  border-radius:16rpx;
  overflow: hidden;
  background: #fff;
  margin-top: 20rpx;
} 
.cardimage{
  width: 120rpx;
  height: 120rpx;
}

.newcard{
  width:40%;
  height: 64rpx;
  line-height: 64rpx;
  border: solid 2rpx #666;
  color: #fff;
  margin: 20rpx auto;
  background: #0066eb;
  border-radius: 70rpx;
  font-size: 26rpx;
} 
.show{
  display: block;
}
.hide{
  display: none;
}
.nav {
