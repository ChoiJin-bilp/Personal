/* pages/about/about.wxss */
.Company-logo{
  width: 100%;
  height: 280rpx;
  position: relative;
  text-align: center;
}
.logo{
  width: 126rpx;
  height: 60rpx;
  position: absolute;
  left: 2%;top: 20rpx; 
}
.logo-image{
  /* width: 100rpx;
  height: 100rpx; */
  height: 100rpx;
  margin: 0 auto;
  margin-top: 80rpx;
  margin-bottom: 20rpx;
}
.edition{
  width: 100%;
  color: #999;
  font-size: 0.8rem;
  text-align: center;
  padding-bottom: 20rpx;
  
}
.edition text{
  color: #0066eb;
  margin-left: 20rpx;
  text-decoration: underline;
}
.edition image{
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin: -2rpx 0 0 20rpx;
}
.comp-title{
  width: 100%;
  height: 72rpx;
  border-left: solid 12rpx #0066eb;
  box-sizing: border-box;
  position: relative;
  clear: both;
  margin-top: 40rpx;
}
.comp-title-name{
  position: absolute;
  left: 12rpx;
  top: -8rpx;
  font-size: 0.95rem;
  font-weight: bold;
  color: #666;
}
.comp-title-english{
  position: absolute;
  left: 12rpx;
  bottom:-6rpx;
  font-size: 0.7rem;
  color: #999;
  text-transform: Capitalize;
}
.introduce{
  display: block;
  width: 92%;
  margin: 0 auto;
  font-size: 0.85rem;
  color: #666;
  padding: 20rpx 0 0 0;
  text-align: justify;
  line-height: 1.4rem;
}
.Contact{
  width: 92%;
  margin: 0 auto;
  font-size: 0.85rem;
  padding-top: 20rpx;
  position: relative;
}
.Contact-way{
  display: inline-block;
  width: 70%;
  line-height: 1.4rem;
  color: #666;
}
.qrcode{
  position: absolute;
  right: 0;
  top: 20rpx;
  width: 200rpx;
}
.qrcode image{
  width: 200rpx;
  height: 200rpx;
}
.qrcode text{
  display: block;
  width: 100%;
  height: 50rpx;
  margin-top: -10rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 1rem;
}
.product{
  width:92%;
  margin: 0 auto;
  padding-bottom: 20rpx;
  margin-top: 20rpx;
}
.a-product{
  float: left;
  width: 25%;
  text-align: center;
  padding: 0 0 20rpx 0;
  border-bottom: solid 2rpx #f5f5f5; 
}
.a-product image{
  vertical-align: middle;
  width: 100rpx;
  height: 100rpx;
}
.a-product text{
  display: block;
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 0.7rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #666;
  margin-top: 10rpx;
}
/*弹出*/
.userule{
  position: fixed;
  left: 0;top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
}

.recordbox{
  width:90%;
  height:70%;
  position: relative;
  top:50%;left:50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
  overflow: hidden;
  background: #fff;
  border-radius: 16rpx;
}
.record{
  height:88%;
  position: absolute;
  top:3%;left:4%;
  overflow-y:scroll;
}

.delete-box{
  width:100%;
  height:60rpx;
  position: absolute;
  bottom:10rpx;right:0;
  z-index: 99999;
  text-align: center;
}
.del-record{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
}
.versions{
  width: 92%;
  margin: 0 auto;
  clear: both;
  margin-top: 20rpx;
}
.update{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 1rem;
  font-weight:500;
  color: #333; 
}
.versondate{
  font-size: 0.85rem;
  font-weight:500;
  margin-left: 50rpx;
  color: rgb(70, 69, 69); 
}
.update-nav{
  width: 90%;
  margin-left: 10%;
  line-height: 40rpx;
  padding: 10rpx 0;
  font-size: 0.7rem;
  color: #666;
}
.aboutlogo{
 width: 100%;
 height: 650rpx;
 display: flex;
 justify-content: center;
 align-items: center;
}
.logo-img {
  width: 180rpx;
  height: 180rpx;
  margin: 0 auto;
  margin-top: 50rpx;
  margin-bottom: 30rpx;
}
.abogo{
  width: 100%;
 padding: 0 50rpx;
 font-size: 0.86rem;
 box-sizing: border-box;
}
.aboutcompany{
  color: #4A515A;
  margin-bottom: 20rpx;
  text-indent: 2.2rem;line-height: 42rpx;
  font-size: 30rpx;
}
.website{
  color: #B7B7B7;
  line-height: 60rpx;
  font-size: 30rpx;
}
.version{
  display: flex;
  width:92%;
 justify-content: center;
 align-items: center;
  color: #B7B7B7;
 font-size: 0.83rem;
 box-sizing: border-box;
 border-top: rgb(189, 187, 187) solid 1rpx;
 height: 100rpx;
 margin-left: 3%;
 font-size: 30rpx;
}
.bus{
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction:column; 
}