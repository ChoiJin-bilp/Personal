    @keyframes showZeroAlert {
  0% {
    transform: rotateZ(5deg);
  }
      25% {
    transform: rotateZ(0deg);
  }
        50% {
    transform: rotateZ(-5deg);
  }
    100% {
    transform: rotateZ(0deg);
  }
}
@keyframes showZero{
  0%{
  transform: rotateZ(-5deg);
  }  100%{
  transform: rotateZ(5deg);
  }
}
