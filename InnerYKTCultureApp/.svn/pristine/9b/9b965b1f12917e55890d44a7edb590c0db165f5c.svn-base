/* packageVP/pages/mkghetrs/mkghetrs.wxss */
page{
  padding: 20rpx 40rpx;
  box-sizing: border-box;
  background: #F6F6F6;
}
.lrte{
  width: 100%;
  height: 260rpx;
  opacity: 1;
  background: #ffffff;
  border-radius: 10rpx;
  box-shadow: 1rpx 1rpx 6rpx 0rpx rgba(218,218,218,0.80); 
  display: flex;
  justify-content: space-around;
  align-items: center;
color: #333333;
}
.lpor{
text-align: center;
}
.lpor image{
  width: 80rpx;
height: 70rpx;
margin-bottom: 20rpx;
}
.lpor:nth-of-type(2)  image{
height: 80rpx;
}