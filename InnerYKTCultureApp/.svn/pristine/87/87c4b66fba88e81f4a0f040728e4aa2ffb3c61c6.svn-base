/* pages/shopadd/shopadd.wxss */
.cart-box{
    padding-top: 80rpx;
}
.cart-list{
    position: relative;
    padding: 30rpx 100rpx 30rpx 80rpx;
    height: 80rpx;
    border-bottom: 1rpx solid #e9e9e9;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 48rpx;
    width: 44rpx; 
    height: 44rpx;
}

.cart-infor{
  width: 100%;
  height: 100rpx;
  overflow: hidden;
}
.use-order{
  width: 100%;
  height: 40rpx;
  line-height:40rpx;
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #333; 
  /* font-weight: bold;  */
}
.use-order text{
  font-size: 28rpx;
  color: #666;
  font-weight: normal;
  margin-left: 20rpx;
}
.use-address{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 26rpx;
  color: #333;
}

.cart-header{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 80rpx;
    padding:0 100rpx 0 80rpx;
    box-sizing: border-box;
    background: #f8f8f8;
}
.cart-header text{
  display: inline-block;
  color: #333;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  vertical-align: middle;
}
.cart-header view.butbox{
  position: absolute;
  right: 20rpx;
  top: 16rpx;
  color: #136fe6;
  font-size: 28rpx;
  height: 48rpx;
  line-height: 48rpx;
}
.butbox button{
  display: inline-block;
  height: 48rpx;
  padding: 0 30rpx;
  border-radius:24rpx;
  box-sizing: border-box;
  border: solid 2rpx #136fe6;
  line-height: 44rpx;
  font-size: 26rpx;
  color: #136fe6;
  margin-left: 20rpx;
}
.butbox button:first-child{
  margin-left: 0;
}
.total-select{
    position: absolute;
    left: 20rpx;
    top: 20rpx;
    width: 44rpx;
    height: 44rpx;
    vertical-align: middle;
}
.order-icon{
    position: absolute;
    right: 40rpx;
    top: 25rpx;
    width: 44rpx; 
    height: 44rpx;
}
.order-icon image,.order-icon navigator{
  display: block;
  width: 44rpx; 
  height: 44rpx;
}
.cart-toatl-price{
    float: right;
    width: 120rpx;
}

.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}

.cart-footer{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 96%;
    height: 120rpx;
    padding:0 2%;
    background: #eee;
}
.shoppay{
  width: 320rpx;
  float: left;
  height: 80rpx;
  margin:20rpx; 
}
.total{
  width: 100%;
  height: 40rpx;
  line-height: 36rpx;
  font-size: 30rpx;
  color: #000;
}
.total text{
  font-size: 28rpx;
  color: #ff1f1f;

}
.total text text{
  font-size: 36rpx;
  font-weight: bold;

}
.privilege{
  width: 100%;
  height: 40rpx;
  line-height: 44rpx;
  font-size: 30rpx;
  color: #666;
}
.privilege text{
  font-size: 24rpx;
  color: #666;

}
.now-pay{
  display: inline-block;
  width: 220rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #fff;  
  text-align: center;
  background: -webkit-linear-gradient(left, #ff1f1f , #ff592d); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #ff1f1f, #ff592d); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #ff1f1f, #ff592d); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #ff1f1f , #ff592d); /* 标准的语法（必须放在最后） */ 
  border-radius: 40rpx;
   margin: 20rpx;
}
.default{
  position: absolute;
  right: 20rpx;
  top: 30rpx;
  width: 60rpx; 
  height: 60rpx;
  line-height: 60rpx;
  text-align: center; 
  font-size: 22rpx;
  background: #fff;
  border-radius: 50%;
  color: #999;
  border: solid 2rpx #e9e9e9;
  box-sizing: border-box;
}
.BGdefault{
  position: absolute;
  right: 20rpx;
  top: 40rpx;
  width: 60rpx; 
  height: 60rpx;
  line-height: 60rpx;
  text-align: center; 
  font-size: 22rpx;
  background: #136fe6;
  color: #fff;
  border: solid 2rpx #136fe6;
  box-sizing: border-box;
  border-radius: 50%;
}

.userule{
  position: fixed;
  left: 0;top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
}
.bgopcity{
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.4;
  position: absolute;
  left: 0; top: 0;
  z-index: 105;
}
.recordbox{
  width:92%;
  max-height:90%;
  position: relative;
  top:50%;left:50%;
  padding: 10rpx;
  transform: translate(-50%,-60%);
  z-index: 9999;
  overflow: hidden;
  background: #fff;
  border-radius: 16rpx;
}
text.tilte{
  display:inline-block;
  height: 60rpx;
  line-height: 60rpx;
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  color: #333,
}
.ListBoxItem{
  width:100%;
  height: 60rpx;
  margin-top: 30rpx;
  position: relative;
  overflow: hidden;
}
.ListBoxItem text{
  width: 140rpx;
  display: inline-block;
  font-size: 28rpx;
  color: #999;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  left: 20rpx;
  top: 0;
}
.ListBoxItem input,picker{
  display: inline-block;
  width:508rpx;
  padding: 0 10rpx;
  height: 60rpx;
  box-sizing: border-box;
  line-height: 58rpx;
  border-radius: 12rpx;
  border: solid 2rpx #e9e9e9;
  font-size: 28rpx;
  color: #333;
  margin-left: 160rpx;
}
.xiala{
  width: 28rpx;
  height: 16rpx;
  position: absolute;
  right: 40rpx;
  top: 22rpx;
  z-index: 1100;
}

.guidancepop-btn{
  width: 100%;
  height: 100rpx;
  background: #fff;
  text-align: center;
  margin-top: 10rpx;
}
.guidancepop-btn button{
  display: inline-block;
  padding: 0 40rpx; 
  height: 60rpx;
  margin-top: 20rpx;
  line-height: 56rpx;
  border-radius: 60rpx;
  background: #fff;
  box-sizing: border-box;
  margin-left: 40px;
  font-size: 26rpx;
  border: solid 2rpx #0066eb;
  color: #0066eb
}
.guidancepop-btn button:first-child{
  margin-left: 0;
}