}
.radio{
  display: flex;
  align-items: center;
  height: 50rpx;
  white-space: nowrap;
}
.radio radio:nth-of-type(2){
  margin-left: 30rpx;
}
.radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 50%; /* 圆角 */
  width: 20rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 20rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 30rpx;
  text-align: center;
  font-size: 0rpx; /* 对勾大小 30rpx */
  color: #fff; /* 对勾颜色 白色 */
  background: #0065E8;
  transform: translate(-50%, -50%) scale(1);
}
.radio .wx-radio-input.wx-radio-input-checked {
  border-color: #03a9f4;
  background: #fff;
  height: 40rpx;
  width: 40rpx;
}
.radio .wx-radio-input {
  height: 40rpx;
  width: 40rpx;
  margin-top: -4rpx;
  border-radius: 50%;
  border: 2rpx solid #999;
  background: transparent;
}


