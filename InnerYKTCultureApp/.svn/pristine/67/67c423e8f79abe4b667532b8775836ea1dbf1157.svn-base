/* packageVP/pages/callServe/callServe.wxss */
page{
  background: #f6f6f6;
  padding-bottom: 74px;
}
.messetop text {
  font-size: 24rpx;
  left: 64rpx;
  position: absolute;
  white-space: nowrap;
  z-index: 3;
}
.plit{
  overflow: hidden;
    position: relative;
    width: 100%;
    height: 100%;
}
.messetop{
  height: 62rpx;
background: #efefef;
display: flex;
align-items: center;
padding: 15rpx 30rpx;
box-sizing: border-box;
font-size: 28rpx;
color: #666666;
overflow: hidden;
margin-top:10rpx;
}
.messetop image{
  position: absolute;
  left: 0rpx;
width: 36rpx;
height: 100%;
margin-right: 20rpx;
z-index: 4;
background-color:#efefef ;
padding-right: 20rpx;
}
/*当前位置css*/
.spruView{
  display: flex;
  font-size: 30rpx;
color: #333333;
align-items: center;
justify-content: center;
}
.spruView input{
  width: 367rpx;
height: 60rpx;
opacity: 1;
background: #ffffff;
border: 2rpx solid #cccccc;
border-radius: 8rpx;
margin-right: 20rpx;
padding: 0rpx 10rpx;
box-sizing: border-box;
text-align: center;
}
.loadView{
width: 94%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 20rpx auto;
}
.loadView image{
  width: 54rpx;
height: 46rpx;
}
/*--设备问题--*/
.bgdr{
  width: 96%;
opacity: 1;
background: #ffffff;
border-radius: 10rpx;
box-shadow: 0rpx 2rpx 7rpx 0rpx rgba(203,203,203,0.50); 
margin: auto;
padding: 24rpx 38rpx;
box-sizing: border-box;
}
.bgdr .model:nth-of-type(1){
  padding-top: 0rpx;
}
.bgdr .model:nth-of-type(4){
  border-bottom: solid #e5e5e5 0rpx;
}
.bgdr2{
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 0;
}
.model{
  color: #333;
  font-size: 30rpx;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
  border-bottom: solid #e5e5e5 2rpx;
}
.seucces{
  font-size: 26rpx;
 background: #EFF0F2;
 display: flex;
 justify-content: center;
 align-items: center;
  padding: 10rpx 20rpx;
 color: #4B5156;
 margin-right: 20rpx;
 margin-bottom: 20rpx;
 margin-top: 20rpx;
 position: relative;
 border-radius: 8rpx;
}
.selectSucces{
  background: #CEEBE6;
  color: #1B9777;
 }
 .selectCup {
  width: 100%;
  display: flex;
  flex-wrap:  wrap;
}
.model textarea{
  height: 200rpx;
  margin-top: 20rpx;
  background: #ffffff;
  border: 2rpx solid #cccccc;
  border-radius: 8rpx;
  width: 100%;
}
.close-botton {
  display: flex;
  width: 100%;
  height: 80rpx;
  align-items: center;
  justify-content: center;
  color: #ffffff;
  background: #018766;
  border-radius: 48rpx;
}
.Cupup{
  margin-top: 20rpx;
  justify-content: space-around;
}
.close-botton2{
  background: #7F7F7F;
}