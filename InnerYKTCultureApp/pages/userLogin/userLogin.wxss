/* pages/userLogin/userLogin.wxss */
.login{
  width: 100%;
  position: fixed;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding:20rpx;
  box-sizing: border-box;
  padding-bottom: 100rpx;
}
.login-top{
  width: 100%;
  padding-left:20rpx; 
  height: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom:100rpx; 
}
.login-top image{
  width: 250rpx;
  height: 250rpx;
}

.adddvice-s-s{
  width: 100%;
  display: flex;
  justify-content: center;
  /* justify-content: flex-start; */
  color: #999;
  font-size: 0.8rem;
  align-items: center;
  padding: 10rpx;
}
.adddvice-s-s input{
  border: #e5e5e5 solid 1rpx;
  box-sizing: border-box;
  border-radius: 10rpx; 
  color: #333;
  width: 70%;
  height: 76rpx;
  padding-left: 20rpx;
}
.adddvice-s-s view{
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  width: 20%;
}
.anniu{
  background: #0066EB;
  padding: 5rpx 30rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  width: 40%;
  height: 72rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  color: #fff;
  margin: 0rpx auto;
  margin-top: 80rpx;
  margin-left: 50%;
  transform: translateX(-50%);
}