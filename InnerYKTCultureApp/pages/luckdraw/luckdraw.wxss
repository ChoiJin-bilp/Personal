/*弹窗*/
.tanChuan{
  z-index: 200;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.7;
  pointer-events: all;
}
.tanChuan-v{
  padding: 40rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 92%;
  background: #fff;
  border-radius: 20rpx;
  padding-bottom:58rpx;
  padding-top:0rpx;
  top:50%;
  left:50%;
  transform: translateX(-50%) translateY(-55%);
  position: fixed;z-index: 250;
}
.tanChuan-v-t{
  font-size: 36rpx;
  width: 100%;
  padding:0;
  max-height: 800rpx;
  overflow-y: auto;
  box-sizing: border-box;
}
.title{
  width: 100%;
  text-align: center;
  font-size: 34rpx;
  color: #333;
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  font-weight: bold;
  margin-top: 40rpx;
  margin-bottom: 30rpx;
}
.struction{
  box-sizing: border-box;
  width: 100%;
}
.content{
  text-indent: 1rem;
  color: #666;
  line-height: 48rpx;
  font-size: 30rpx;
}
.close{
  position: absolute;
  bottom: 80rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 250rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background: #fff;
  color: #000;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 30rpx auto;
  font-size: 60rpx;
}
.close-o{
  position: absolute;
  left: 50%;
  top: 50%;
  width: 60%;
  border-bottom: rgb(134, 99, 99) solid 4rpx;
  transform: translateX(-50%) translateY(-50%) rotate(45deg);
}
.close-t{
  position: absolute;
  left: 50%;
  top: 50%;
  width: 60%;
  border-bottom: rgb(134, 99, 99) solid 4rpx;
  transform: translateX(-50%) translateY(-50%) rotate(135deg); 
}
