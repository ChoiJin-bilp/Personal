.shanshuo{
  animation:loosh  3s cubic-bezier(1, 1, 1, 1) infinite;
  border-radius:20rpx; 
}
@keyframes loosh {
  0% {
    box-shadow: 0 0 0 0rpx rgba(253, 228, 1, 0.7);
  }
  50%{
    box-shadow: 0 0 0 15rpx rgba(253, 228, 1, 0.5);
  }
  100% {
    box-shadow: 0 0 30rpx rgba(253, 228, 1, 0);
  }
}
