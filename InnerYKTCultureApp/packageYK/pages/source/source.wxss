  width: 58%;
}

.sourc-o image {
  width: 35rpx;
  height: 35rpx;
}

.sourc-t {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
}

.sourc-t-v {
  display: flex;
