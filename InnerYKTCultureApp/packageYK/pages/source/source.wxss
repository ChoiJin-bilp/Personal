.detail-s-o view {
  width: 100%;
  text-align: center;
  padding: 30rpx 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
  position: relative;
  box-sizing: border-box;
}

.detail-s-o view image {
  position: absolute;
  top: 10rpx;
  right: 0;
  width: 36rpx;
  height: 36rpx;
}

.colour {
  color: #2B7CED;
  background: #fff;
}

.sourc-s-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: #999 solid 1rpx;
  padding: 20rpx 0 20rpx 0;
  font-size: 28rpx;
}

.sourc-s-t {
  color: #999;
  font-size: 28rpx;
}

.item-sourc {
  width: 38rpx;
  height: 38rpx;
  border: #ccc solid 1rpx;
  border-radius: 50%;
}

.all::after {
  content: "";
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
  width: 18rpx;
  height: 18rpx;
  background: #0166EA;
  border-radius: 50%;
}

.all {
  position: relative;
  border: #0166EA solid 1rpx;
}

.item-img image {
  width: 130rpx;
  height: 130rpx;
}

.sourc-s-item view:nth-of-type(2) {
  width: 28%;
  text-align: center;
}

.sourc-s-item view:nth-of-type(3) {
  width: 22%;
  white-space: normal;
  word-wrap: break-word;
}

.sourc-s-item view:nth-of-type(4) {
  text-align: center;
  width: 15%;
  word-wrap: break-word;
}

.sourc-s-item view:nth-of-type(5) {
  width: 10%;
  text-align: center;
}

.List {
  overflow-y: auto;
  height: 890rpx;
}

.tanChuan {
  z-index: 200;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #7B7B7B;
  opacity: 0.6;
}

.tanChuan-v {
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 92%;
  background: #fff;
  border-radius: 20rpx;
  padding-bottom: 58rpx;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
  position: fixed;
  z-index: 9999;
}

.tanChuan-v view {
  margin-top: 40rpx;
}

.tanChuan-v-o {
  width: 100%;
  text-align: center;
  font-size: 38rpx;
  font-weight: bold;
}

.tanChuan-v-t {
  font-size: 36rpx;
  width: 100%;
  padding: 0 60rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.porltu{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
}
.tanChuan-v-t input {
  width: 70%;
  font-size: 28rpx;
  border: #ccc solid 2rpx;
  padding-left: 20rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 6rpx;
}

.tanChuan-v-s {
  display: flex;
  font-size: 32rpx;
  width: 100%;
  justify-content: space-around;
  align-items: center;
}

.tanChuan-v-s view {
  font-size: 38rpx;
  padding: 0 20rpx;
  width: 25%;
  line-height: 70rpx;
  border-radius: 10rpx;
  text-align: center;
  border: 2rpx solid #999;
}

.tanChuan-v-s view:nth-of-type(2) {
  background: #169BD5;
  color: #fff;
  border: 2rpx solid #fff;

}
