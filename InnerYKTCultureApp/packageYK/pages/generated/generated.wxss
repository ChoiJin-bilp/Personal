/*下拉样式*/
.option_box {
  position: absolute;
  top: 42rpx;
  width: 100%;
  /* border: 1px solid #efefef; */
  box-sizing: border-box;
  overflow-y: auto;
  border-top: 0;
  background: #fff;
  transition: height 0.3s;
  z-index: 100;
  left: 0rpx;
  max-height: 188rpx;
}

.option {
