/* packageSMall/pages/merchant/merchant.wxss */
/* 顶部 */
page {
  height: 100%;
  background: #f6f6f6;
}

.pages {
  width: 100%;
  height: 100rpx;
  padding: 0 20rpx;
  position: relative;
  box-sizing: border-box;
  background: white;
}

.top-text {
  float: left;
}

/* 下拉框 */

.top-selected {
  width: 25%;
  display: flex;
  float: left;
  align-items: center;
  justify-content: space-between;
  border: 1px solid #ccc;
  padding: 0 5% 0 10rpx;
  height: 66rpx;
  line-height: 66rpx;
  margin-top: 15rpx;
  font-size: 30rpx;
  border-radius: 12rpx;
}
.top-selected  text{
  display: inline-block;
  width: 100%;
  height: 66rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space:  nowrap;
  color: #999;
  font-size: 28rpx;
}
/* 下拉内容 */

.select_box {
  background-color: #fff;
  width: 30%;
  float: left;
  position: absolute;
  left:19rpx;
  top:88rpx;
  z-index: 1;
  overflow: hidden;
  text-align: left;
  animation: myfirst 0.5s;
  border: 1px solid #ccc;
  border-bottom: none;
  font-size: 30rpx;
  border-radius: 12rpx;
}

.select_one {
  padding-left: 10rpx;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom:1px solid #ccc; 
}

/* 下拉过度效果 */

@keyframes myfirst {
  from {
    height: 0rpx;
  }

  to {
    height: 240rpx;
  }
}

/* 下拉图标 */

.top-selected image {
  height: 16rpx;
  width: 28rpx;
  position: absolute;
  left:28%;
  top: 42rpx;
}

/* 搜索栏 */
.add-phone{
  float: right;
  width: 66%;
  height: 68rpx;
  background: #fff;
  margin:15rpx 0;
  border: solid 1px #ccc;
  position: relative;
  border-radius: 12rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.add-phone .search{
  height: 68rpx;
  width:84%;
  padding: 0 1%;
  line-height: 64rpx;
  font-size: 28rpx;
  padding-left:3%; 
  position: absolute;
  left: 0%;
  top: 0;
  z-index: 5;
}
.add-phone image{
  position: absolute;
  right: 0;
  top: 0r;
  width: 42rpx;
  height:42rpx;
  z-index: 10;
  padding: 13rpx 15rpx;
}

