page{
  padding-bottom: 140rpx;
}
.moredetails,.otherdetails{
  width: 100%;
}
.title-more{
  width: 94%;
  padding: 0 3%;
  height: 58rpx;
  line-height: 48rpx;
  text-align: left;
  font-size: 28rpx;
  font-weight: bold;
  box-shadow: 0px 2px 2px #ddd;
  color: #2e2e2e;
}
.more-input{
  width: 92%;
  clear: both;
  padding: 0 4%;
  line-height: 76rpx;
  border-top: solid 2rpx #f5f5f5;
}
.more-input:first-child{
  border-top: none;
}
.more-input-type{
  width: 20%;
  text-align: left;
  height: 76rpx;
  font-size: 28rpx;
  line-height: 76rpx;
  color: #0066e9;
}
.more-input-infor{
  width: 80%;
  text-align: left;
  font-size: 28rpx;
  color: #2e2e2e;
  height: 76rpx;
  line-height: 76rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.other-input{
  width: 92%;
  padding: 0 4%;
  border-bottom: solid 2rpx #f5f5f5;
}
.other-input-type{
  font-size:28rpx;
  color: #0066e9;
  height: 60rpx;
  line-height: 60rpx;
}
.other-input-infor{
  width: 100%;
  padding: 10rpx 0; 
  font-size:28rpx;
  color: #2e2e2e;
}
.other-input-image{
  width: 100%;
}
