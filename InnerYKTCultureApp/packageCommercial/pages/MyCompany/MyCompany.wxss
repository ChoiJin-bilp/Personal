/* pages/MyCompany/MyCompany.wxss */
page{
  background: #f9f9f9;
}
.tab{
  float: left; 
  width: 24%;
  height: 70rpx;
  margin-left: 7%;
  text-align: center;
  box-sizing: border-box;
}
.tab:last-child{
  border-right: 0;
}
.topTabSwiper{
  position: fixed;left: 0;top: 0;
  width: 100%;
  z-index: 2;
  background: #fff;
  height: 72rpx;
  line-height: 72rpx;
}
.topTabSwiper:after{content: ""; clear: both;display: block;}
.tabBorer{
  border-bottom: 1px solid #0066eb; 
  color: #0066eb;
  box-sizing: border-box;
}
.swiper{width: 100%;padding-top:72rpx; }
.swiper_con{text-align: center; width: 100%; height: 100%;}
swiper-item{overflow: hidden;}
.add-phone{
  width: 100%;
  height: 100%;
  overflow-y: scroll;
  position: relative;
}
.mycard-title{
  position: fixed;
  left: 0; 
  top: 0;
  height: 70rpx;
  padding: 0 4%; 
  line-height: 70rpx;
  font-size: 30rpx;
  background: #fff;
  z-index: 999;
  text-align: center;
}
.mycard-title image{
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
  padding: 11rpx 20rpx;
}
.mycard-title text{
  display: inline-block;
  height: 48rpx;
  line-height: 48rpx;
  padding: 11rpx 0 11rpx 10rpx;
}
.mycard-title button{
  height: 60rpx;
  width: 30%;
  line-height: 60rpx;
  border: solid 2rpx #0066eb;
  color: #0066eb;font-size: 28rpx;
  margin-top: 10rpx;
  background: #fff;
  border-radius: 60rpx;
}
/* .mycard-title image{
  float: right;
  height: 60rpx;
  width: 60rpx;
  margin-top: 10rpx;
} */
.nav-box{
  width: 94%;
  padding: 0 3%;
}
.A-company{
  width: 100%;
  height: 180rpx;
  border:  2rpx solid #e5e5e5;
  margin-top: 16rpx;
  border-radius: 16rpx;
  overflow: hidden;
  position: relative;
}
.Default{
  position: absolute;
  right: -60rpx;
  top: 16rpx;
  width: 180rpx;
  height: 36rpx;
  line-height: 36rpx;
  background: #e5e5e5;
  color: #666;
  font-size: 24rpx;
  text-align: center;
  transform:rotate(45deg);
 }
.A-company:last-child{
  margin-bottom: 20rpx;
}
.head-company,.reception{
  width:92%;
  height:52rpx;
  line-height: 50rpx;
  margin: 0 auto; 
  font-size: 30rpx;
}
.reception text,.reception image{
  display: inline-block;
}
.reception image{
  vertical-align: middle;
  width: 28rpx;
  height: 16rpx;
  margin-top: -4rpx;
  padding: 16rpx 20rpx;
  margin-left: 10rpx;
}
.head-image{
  position: relative;
  width: 160rpx;
  height: 40rpx;
  text-align: center;
}
.head-image image{
  position: absolute;
  width: 160rpx;
  height: 40rpx;
  right: 0;
  top: 10rpx;
}
.head-image text{
  display: block;
  position: absolute;
  width: 160rpx;
  height: 40rpx;
  line-height: 40rpx;
  right: 0;
  top: 10rpx;
  font-size: 28rpx;
  color: #0066eb;
}
.foot-company{
  width:92%;
  padding: 4rpx 2%;
  height:110rpx;
  margin: 0 auto; 
  background: #fff;
  border-radius: 16rpx;
}
.use-data,.receive-data{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.use-data text:first-child,.receive-data text:first-child{
  margin-right: 40rpx;
}
.use-company,.receive-company{
  height: 46rpx;
  line-height: 46rpx;
  font-size: 30rpx;
  color: #333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.receive-time{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #333;
  text-align: right;
}
.receive,.receive1{
  width:92%;
  padding: 3rpx 2%;
  height:140rpx;
  margin: 0 auto; 
  background: #fff;
  margin-top: 16rpx; 
  border-radius: 16rpx;
  overflow: hidden;
}
.receive1{
  border: solid 2rpx #e5e5e5;
}
.send-company{
  width: 96%;
  padding: 10rpx 1%;
  border: solid 2rpx #e5e5e5;
  margin: 0 auto;
  box-sizing: border-box;
  border-radius: 16rpx;
  overflow: hidden;
  margin-top: 16rpx;
 }

 /*弹出*/
.userule{
  position: fixed;
  left: 0;top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
}
.tipsbox{
  width:100%;
  position: relative;
  top:50%;left:50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
  max-height: 80%;
  border-radius: 8px;
  background: #fff;
  padding-bottom: 100rpx;
  overflow: hidden;
}
.pop-array{
  width: 100%;
  overflow-y: scroll;
}

.chose-txt{
  border-radius: 8px; 
  width: 92%;
  margin: 16rpx auto;
  padding-bottom: 10rpx;
  overflow: hidden;
  clear: both;
}

.pop-btns{
  position:absolute;
  width: 100%;
  height: 100rpx;
  background: #fff;
  bottom:0rpx;
  left: 0;
  z-index: 99999;
}
.pop-btns button{
  width: 30%;
  height: 60rpx;
  margin-top: 20rpx;
  line-height: 60rpx;
  float: left;  
  border-radius: 60rpx;
  border: solid 2rpx #e5e5e5;
  box-sizing: border-box;
  margin-left: 13.3333%;
  font-size: 30rpx;
}

.btn-qx{
  background: #f9f9f9;
  color: #999;
}
.btn-qd{
  border: solid 2rpx #0066e9 ;
  color: #0066e9 ;
}

.delete-box{
  width:100%;
  position: absolute;
  text-align: center;
  height: 100rpx;
  background: #fff;
  bottom:0rpx;
  left: 0;
  z-index: 99999;
}
.del-record{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  padding: 20rpx 0;
}