  text-align: center;
  font-size: 28rpx;
  color: #333;
}

checkbox-group {
  width: 66%;
  float: left;
  font-size: 28rpx;
}

.redbackokpop {
  width: 90%;
  position: relative;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 9999;
  background: #fff;
  border-radius: 8px;
  overflow: hidden;
}

.redokbox {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333;
  padding: 20rpx 0;
}

.redoktips {
  display: block;
  width: 90%;
  margin: 10rpx auto;
  line-height: 48rpx;
  font-size: 28rpx;
  color: #999;
  text-align: justify;
}

/* 未选中的 背景样式 */

checkbox .wx-checkbox-input {
  border-radius: 50%; /* 圆角 */
  width: 44rpx; /* 背景的宽 */
  height: 44rpx; /* 背景的高 */
}

/* 选中后的 背景样式 （红色背景 无边框 可根据UI需求自己修改） */

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: solid 2rpx #09bb07;
  background: #09bb07;
}

/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%; /* 圆角 */
  width: 60rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 60rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx; /* 对勾大小 30rpx */
  color: #fff; /* 对勾颜色 白色 */
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.Please-topup {
  width: 100%;
  margin-bottom: 20rpx;
  color: red;
  height: 60rpx;
  background: #ffcdcd;
  text-align: center;
  line-height: 60rpx;
  border-radius: 8rpx;
}

.Please-topup text {
  text-decoration: underline;
}

.hangye {
