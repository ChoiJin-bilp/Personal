/* components/floatinball/floatinball.wxss */
movable-area{
  pointer-events:none;
    z-index: 100;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
} 
movable-view{
  pointer-events:auto;
  position: absolute;
  z-index: 1400;
  height: 130rpx;
  width: 130rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #305985;
  border-radius:50%; 
  opacity: 0.6;
  border: #ccc solid 10rpx;
}
movable-view view{
  font-size: 38rpx;
  border-radius: 50%;
  text-align: center;
  color: #fff;
  width: 100rpx;
  height: 100rpx;
}